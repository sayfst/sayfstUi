 Main:CreateButton('Pizza/Chiken', function()
     Now = Character.HumanoidRootPart.Position
 
     for i,v in pairs(Player.Backpack:GetChildren()) do
         if v:IsA("Tool") then
             if v.Name == '[Pizza]' or v.Name == '[Chicken]' then
                 v.Parent = game.Players.LocalPlayer.Character
             end
         end
     end
 
     repeat
         Teleport(CFrame.new(game:GetService("Workspace").Ignored.Shop["[Pizza] - $5"].Head.Position))
         fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Pizza] - $5"].ClickDetector)
         wait()
     until Player.Backpack:FindFirstChild('[Pizza]') ~= nil
     repeat
         if Player.Backpack:FindFirstChild('[Chicken]') then
             Player.Backpack:FindFirstChild('[Chicken]').Parent = game.Players.LocalPlayer.Character
         end
         Teleport(CFrame.new(game:GetService("Workspace").Ignored.Shop["[Chicken] - $7"].Head.Position))
         fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Chicken] - $7"].ClickDetector)
         wait()
     until Player.Backpack:FindFirstChild('[Chicken]') ~= nil
     Teleport(CFrame.new(Now))
 end)
 
