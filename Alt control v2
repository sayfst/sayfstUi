                                 elseif a == 'Alt24' then
                                    Adicional = 72
                                 elseif a == 'Alt25' then
                                    Adicional = 75
                                 elseif a == 'Alt26' then
                                    Adicional = 78
                                 elseif a == 'Alt27' then
                                    Adicional = 81
                                 elseif a == 'Alt28' then
                                    Adicional = 84
                                 elseif a == 'Alt29' then
                                    Adicional = 87
                                 elseif a == 'Alt30' then
                                    Adicional = 90
                                 end
                                 wait()
                                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-871.0387573242188, -38.39921188354492, -545.9679565429688 - Adicional)
                                 wait(1)
                                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-871.0387573242188, -38.39921188354492 + 5, -545.9679565429688 - Adicional)
                                 wait(.1)
                                 Anchored(true)
                            elseif args[2] == 'safezone' then
                                Anchored(false)
                                wait(.4)
                                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(235.09283447265625, 38.25, 200021.453125)
                                 wait(1)
                                 local Adicional
 
                                 if a == 'Alt1' then
                                     Adicional = 3
                                 elseif a == 'Alt2' then
                                     Adicional = 6
                                 elseif a == 'Alt3' then
                                     Adicional = 9
                                 elseif a == 'Alt4' then
                                     Adicional = 12
                                 elseif a == 'Alt5' then
                                     Adicional = 15
                                 elseif a == 'Alt6' then
