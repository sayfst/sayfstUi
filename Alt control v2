     Reach()
 end)
 
 Main:CreateButton('Notification on player enter', function()
     PlayersEntered = not PlayersEntered
     game.StarterGui:SetCore("SendNotification", {
         Title = 'Sucess';
         Text = 'Notification on player enter is:' .. PlayersEntered
     })
 end)
 
 
 
 Main:CreateButton('Stomp player', function()
     Now = Character.HumanoidRootPart.Position
     for i = 1,20 do
         Teleport(CFrame.new(getgenv().Player.Character.UpperTorso.Position + Vector3.new(0, 1.5, 0)))
         Stomp()
         wait()
     end
     wait(.1)
     Character.HumanoidRootPart.CFrame = CFrame.new(Now)
 end)
 
 function Reach()
     LP = game.Players.LocalPlayer
     for i,v in ipairs(LP.Character:GetDescendants()) do
     if v:IsA("MeshPart") then v.Massless = true
     v.CanCollide = false
     v.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0)
     
     end
     end
     
     for i,v in ipairs(game.workspace:GetDescendants()) do
     if v:IsA("Seat") then 
     v.Disabled = true
     end
     end
     x = 35
     y = 35
     z = 35
     
     
     penis = Vector3.new(x,y,z)
     
     LP.Character.RightHand.Size = penis
     
     LP.Character.RightHand.Transparency = 1
     local selectionBox = Instance.new("SelectionBox",LP.Character.RightHand)
     selectionBox.Adornee = LP.Character.RightHand
     selectionBox.Color3 = Color3.new(1,0,0)
     
     LP.Character.LeftHand.Size = penis
     LP.Character.BodyEffects.SpecialParts.LeftHand.Size = penis
     
     LP.Character.LeftHand.Transparency = 1
     local selectionBox = Instance.new("SelectionBox",LP.Character.LeftHand)
     selectionBox.Adornee = LP.Character.LeftHand
     selectionBox.Color3 = Color3.new(1,0,0)
 end
 
 function Kill(player)
     local Player = player
     local Character = game.Players:FindFirstChild(Player).Character
     HideMain()
     local Now = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame function Reset() game.Players.LocalPlayer.Character["HumanoidRootPart"]:Destroy() game.Players.LocalPlayer.Character["RightHand"]:Destroy() game.Players.LocalPlayer.Character["LeftLowerArm"]:Destroy() game.Players.LocalPlayer.Character["RightLowerArm"]:Destroy() game.Players.LocalPlayer.Character["LeftUpperArm"]:Destroy() game.Players.LocalPlayer.Character["RightUpperArm"]:Destroy() game.Players.LocalPlayer.Character["LowerTorso"]:Destroy() game.Players.LocalPlayer.Character["LeftLowerLeg"]:Destroy() game.Players.LocalPlayer.Character["LeftUpperLeg"]:Destroy() game.Players.LocalPlayer.Character["LeftFoot"]:Destroy() game.Players.LocalPlayer.Character["RightFoot"]:Destroy() game.Players.LocalPlayer.Character["RightLowerLeg"]:Destroy() game.Players.LocalPlayer.Character["RightUpperLeg"]:Destroy() game.Players.LocalPlayer.Character["UpperTorso"]:Destroy() game.Players.LocalPlayer.Character["LeftHand"]:Destroy() game.Players.LocalPlayer.Character["Head"]:Destroy() for i,v in pairs(game.Players.LocalPlayer.Character:GetDescentands()) do if v:IsA("Acessory") or v:IsA("Tool") then v:Destroy() end end end  function a() function Stomp() game:GetService("ReplicatedStorage").MainEvent:FireServer('Stomp') end function Kill(player) game.Players.LocalPlayer.Backpack:FindFirstChild('[Knife]').Parent = game.Players.LocalPlayer.Character wait() workspace.CurrentCamera.CameraSubject = player.Character print('View player') local character = player.Character local Killed = false local Now = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame delay(0, function() print('Killing') repeat game.Players.LocalPlayer.Character:FindFirstChild('[Knife]'):Activate() wait(0.8) until Killed == true end) wait(1) repeat game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -7) wait() game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 7) wait() game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame * CFrame.new(7, 0, 0) wait() game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame * CFrame.new(-7, 0, 0) wait() game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame * CFrame.new(7, 0, 7) wait() game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame * CFrame.new(-7, 0, -7) wait() game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame * CFrame.new(-7, 0, 7) wait() game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0) wait()  until player.Character.Humanoid.Health <=5 or game.Players.LocalPlayer.Character.Humanoid.Health <=5 if player.Character.Humanoid.Health <=5 then spawn(function() Killed = true print('Sucess killing') HideMain() print('Sucess stomped') game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = _G.Now   game.Players.LocalPlayer.Character:FindFirstChild('[Knife]').Parent = game.Players.LocalPlayer.Backpack workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character end) end end if game.Players.LocalPlayer.Character.Humanoid.Health <= 5 then Reset() end if game.Players:FindFirstChild(Player) then print('Player found '..Player) local player = game.Players:FindFirstChild(Player) local character = player.Character or player.CharacterAdded:wait() if game.Players.LocalPlayer.Backpack:FindFirstChild('[Knife]') or game.Players.LocalPlayer.Character:FindFirstChild('[Knife]') then print('Killing...') Kill(player) else repeat game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Ignored.Shop["[Knife] - $150"].Head.CFrame fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Knife] - $150"].ClickDetector) wait() print('Buying knife') until game.Players.LocalPlayer.Backpack:FindFirstChild('[Knife]') ~= nil print('Knife sucess buyed') Kill(player) end end end if game.Players.LocalPlayer.Character:FindFirstChild('In-gameMask') then a() else repeat game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Ignored.Shop["[Surgeon Mask] - $25"].Head.CFrame fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Surgeon Mask] - $25"].ClickDetector) wait() until game.Players.LocalPlayer.Backpack:FindFirstChild("Mask") ~= nil game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Now game.Players.LocalPlayer.Backpack:FindFirstChild("Mask").Parent = game.Players.LocalPlayer.Character wait() game.Players.LocalPlayer.Character.Mask:Activate() wait(1) a() end
 end
 
 Main:CreateButton('Kill', function()
     _G.Now = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
     if game.Players.LocalPlayer.Character:FindFirstChild('In-gameMask') then
         Kill(getgenv().Player.Name)
     else
         repeat
             game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Ignored.Shop["[Surgeon Mask] - $25"].Head.CFrame
             fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Surgeon Mask] - $25"].ClickDetector)
             wait()
         until game.Players.LocalPlayer.Backpack:FindFirstChild("Mask") ~= nil
         game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = _G.Now
         game.Players.LocalPlayer.Backpack:FindFirstChild("Mask").Parent = game.Players.LocalPlayer.Character
         wait()
         game.Players.LocalPlayer.Character.Mask:Activate()
         wait(1)
         Kill(getgenv().Player.Name)
     end
 end)
 
 -- Select players
 
 Main:CreateTab('Select players')
 
 Main:CreateButton('Select players', function()
 -- Gui to Lua
 -- Version: 3.2
 
 -- Instances:
 
 local ScreenGui = Instance.new("ScreenGui")
 local Frame = Instance.new("Frame")
 local TextButton = Instance.new("TextButton")
 local UIGridLayout = Instance.new("UIGridLayout")
 
 --Properties:
 
 ScreenGui.Parent = game.CoreGui
 ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
 
 spawn(function()
     while true do
         if getgenv().MenuOn == false then
             ScreenGui:Destroy()
         end
         wait()
     end
 end)
 Frame.Parent = ScreenGui
 Frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
 Frame.Position = UDim2.new(0.26628077, 0, 0.610129595, 0)
 Frame.Size = UDim2.new(0, 645, 0, 311)
 Frame.Active = true
 Frame.Draggable = true
 
 TextButton.Parent = Frame
 TextButton.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
 TextButton.BorderSizePixel = 0
 TextButton.Size = UDim2.new(0, 200, 0, 50)
 TextButton.Visible = false
 TextButton.Font = Enum.Font.SourceSans
 TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
 TextButton.TextSize = 14.000
 
 UIGridLayout.Parent = Frame
 UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
 UIGridLayout.CellSize = UDim2.new(0, 76, 0, 30)
 
 -- Scripts:
 
 local function ZWPY_fake_script() -- Frame.LocalScript 
     local script = Instance.new('LocalScript', Frame)
 
     for i,v in pairs(game.Players:GetChildren()) do
         if v ~= game.Players.LocalPlayer then
             local clone = script.Parent.TextButton:Clone()
             clone.Parent = script.Parent
             clone.Name = v.Name
             clone.TextSize = 12
             clone.Text = v.Name 
             clone.Visible = true
             clone.MouseButton1Click:Connect(function()
                 getgenv().Player = game.Players:FindFirstChild(v.Name)
             end)
         end
     end
     
     game.Players.ChildAdded:Connect(function(v)
         local clone = script.Parent.TextButton:Clone()
         clone.Parent = script.Parent
         clone.Name = v.Name
         clone.TextSize = 12
         clone.Text = v.Name
         clone.Visible = true
         clone.MouseButton1Click:Connect(function()
             getgenv().Player = game.Players:FindFirstChild(v.Name)
         end)
     end)
     game.Players.ChildRemoved:Connect(function(v)
         script.Parent:FindFirstChild(v.Name):Destroy()
     end)
 end
 coroutine.wrap(ZWPY_fake_script)()
 
 end)
