 -- Toggles
 Main:CreateTab('Toggles freatures')
 
 function Pickup(state)
     local args = {
         [1] = "Grabbing",
         [2] = state,
     }
     game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
 end
 function Stomp()
     game:GetService("ReplicatedStorage").MainEvent:FireServer('Stomp')
 end
 
 Main:CreateButton('View', function()
     workspace.CurrentCamera.CameraSubject = getgenv().Player.Character
 end)
 Main:CreateButton('Unview', function()
     workspace.CurrentCamera.CameraSubject = Character
 end)
 Main:CreateButton('Save player', function()
     Now = Character.HumanoidRootPart.Position
     for i = 1,10 do
         Character.HumanoidRootPart.CFrame = CFrame.new(getgenv().Player.Character.UpperTorso.Position + Vector3.new(0, 1.5, 0))
         wait()
     end
     Pickup(true)
     wait(.1)
     Character.HumanoidRootPart.CFrame = CFrame.new(Now)
     wait(1)
     Pickup(false)
 end)
 
 Main:CreateButton('Teleport', function()
     Teleport(CFrame.new(getgenv().Player.Character.UpperTorso.Position + Vector3.new(0, 1.5, 0)))
 end)
 
