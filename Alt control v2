
    local Settings = getgenv().AltControlSettings
    
    if not game:IsLoaded() then
        game.Loaded:wait()
    end
    
    spawn(function()
    local bb=game:service'VirtualUser'
game:service'Players'.LocalPlayer.Idled:connect(function()
bb:CaptureController()bb:ClickButton2(Vector2.new())
end)
game.StarterGui:SetCore('SendNotification', {
                            Title = 'Sucess';
                            Text = 'Anti Idle is enabled!';
                        })
    end)
    
    getgenv().Host = nil
    if game.Players:FindFirstChild(Settings['Host']) then
        getgenv().Host = game.Players:FindFirstChild(Settings['Host'])
    end
    function Say(message)
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(message, 'All')
    end
    
    local Prefix = Settings['Prefix']
    
    local lPlayer = game.Players.LocalPlayer
    local lCharacter = lPlayer.Character
    lPlayer.CharacterAdded:Connect(function(Character)
        lCharacter = Character
    end)
    
   
    
    function Alt(Numero, Distancia, Altura, AlturaHead)
        local a = 'Alt'..Numero
        if game.Players.LocalPlayer.UserId == Settings[a] then
            if getgenv().Host ~= nil then
                delay(5, function()
                    setfpscap(getgenv().AltControlSettings['Fps'])
                    workspace:FindFirstChildOfClass('Terrain').WaterWaveSize=0;workspace:FindFirstChildOfClass('Terrain').WaterWaveSpeed=0;workspace:FindFirstChildOfClass('Terrain').WaterReflectance=0;workspace:FindFirstChildOfClass('Terrain').WaterTransparency=0;game:GetService("Lighting").GlobalShadows=false;game:GetService("Lighting").FogEnd=9e9;settings().Rendering.QualityLevel=1;for a,b in pairs(game:GetDescendants())do if b:IsA("Part")or b:IsA("UnionOperation")or b:IsA("MeshPart")or b:IsA("CornerWedgePart")or b:IsA("TrussPart")then b.Material="Plastic"b.Reflectance=0 elseif b:IsA("Decal")then b.Transparency=1 elseif b:IsA("ParticleEmitter")or b:IsA("Trail")then b.Lifetime=NumberRange.new(0)elseif b:IsA("Explosion")then b.BlastPressure=1;b.BlastRadius=1 end end;for a,b in pairs(game:GetService("Lighting"):GetDescendants())do if b:IsA("BlurEffect")or b:IsA("SunRaysEffect")or b:IsA("ColorCorrectionEffect")or b:IsA("BloomEffect")or b:IsA("DepthOfFieldEffect")then b.Enabled=false end end;workspace.DescendantAdded:Connect(function(c)coroutine.wrap(function()if c:IsA('ForceField')then game:GetService('RunService').Heartbeat:Wait()c:Destroy()elseif c:IsA('Sparkles')then game:GetService('RunService').Heartbeat:Wait()c:Destroy()elseif c:IsA('Smoke')or c:IsA('Fire')then game:GetService('RunService').Heartbeat:Wait()c:Destroy()end end)()end)
           local ba=Instance.new("ScreenGui")
local ca=Instance.new("TextLabel")local da=Instance.new("Frame")
local _b=Instance.new("TextLabel")local ab=Instance.new("TextLabel")ba.Parent=game.CoreGui
ba.ZIndexBehavior=Enum.ZIndexBehavior.Sibling;ca.Parent=ba;ca.Active=true
ca.BackgroundColor3=Color3.new(0.176471,0.176471,0.176471)ca.Draggable=true
ca.Position=UDim2.new(0.698610067,0,0.098096624,0)ca.Size=UDim2.new(0,370,0,52)
ca.Font=Enum.Font.SourceSansSemibold;ca.Text="Anti AFK Alt control v2"ca.TextColor3=Color3.new(0,1,1)
ca.TextSize=22;da.Parent=ca
da.BackgroundColor3=Color3.new(0.196078,0.196078,0.196078)da.Position=UDim2.new(0,0,1.0192306,0)
da.Size=UDim2.new(0,370,0,107)_b.Parent=da
_b.BackgroundColor3=Color3.new(0.176471,0.176471,0.176471)_b.Position=UDim2.new(0,0,0.800455689,0)
_b.Size=UDim2.new(0,370,0,21)_b.Font=Enum.Font.Arial;_b.Text=""
_b.TextColor3=Color3.new(0,1,1)_b.TextSize=20;ab.Parent=da
ab.BackgroundColor3=Color3.new(0.176471,0.176471,0.176471)ab.Position=UDim2.new(0,0,0.158377,0)
ab.Size=UDim2.new(0,370,0,44)ab.Font=Enum.Font.ArialBold;ab.Text="Status: Active"
ab.TextColor3=Color3.new(0,1,1)ab.TextSize=20;local bb=game:service'VirtualUser'
game:service'Players'.LocalPlayer.Idled:connect(function()
bb:CaptureController()bb:ClickButton2(Vector2.new())
ab.Text="Roblox tried to kick u but i kicked him instead"wait(2)ab.Text="Status : Active"end)
                end)
                getgenv().Host.Chatted:Connect(function(message)
                    if getgenv().Host ~= game.Players.LocalPlayer then
                        
                        function Teleport(Position)
                            game:GetService('TweenService'):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(0.3), {CFrame = Position}):Play()
                        end
                        function Teleport2(Position)
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Position
                        end
                        
                         function Anchored(state)
                             lCharacter.HumanoidRootPart.Anchored = state
                         end
                         function Drop(amonut)
                             game:GetService("ReplicatedStorage").MainEvent:FireServer('DropMoney', ''..amonut..'')
                         end
                         function Block(status)
                             local Target = game:GetService("ReplicatedStorage").MainEvent;
                             Target:FireServer("Block", status)
                         end
                         function Wallet(status)
                             if status == true then
                                 game.Players.LocalPlayer.Backpack:FindFirstChild("Wallet").Parent = game.Players.LocalPlayer.Character
                             elseif status == false then
                                 game.Players.LocalPlayer.Character:FindFirstChild("Wallet").Parent = game.Players.LocalPlayer.Backpack
                             end
                         end
                         args = message:split(' ')
                         argsSay = message:split(':')
                         if argsSay[1] == Settings['Prefix'] .. 'say' then
                             Say(argsSay[2])
                         end
     
                         -- Setup
                         local DistanciaPrincipal = Distancia + 5
                         local AlturaPrincipal = Altura + 2
                         local Head = AlturaHead + 2
                         getgenv().OldGravity = workspace.Gravity
                         if args[1] == Prefix .. 'warp' then
                             if args[2] == 'line' then
                                 Anchored(false)
                                 wait(.3)
                                 Teleport(CFrame.new(getgenv().Host.Character.HumanoidRootPart.Position + Vector3.new(DistanciaPrincipal, 0, 0)))
                                 wait(1)
                                 Teleport(CFrame.new(getgenv().Host.Character.HumanoidRootPart.Position + Vector3.new(DistanciaPrincipal, AlturaPrincipal, 0)))
                                 wait(0.3)
                                 Anchored(true)
                             elseif args[2] == 'disconnect' then
                                 Anchored(false)
                                 if getgenv().WarpLoop then
                                     
                                    getgenv().WarpLoop:Disconnect()
                                    workspace.Gravity = getgenv().OldGravity
                                 end
                             elseif args[2] == 'loop' then
                                Anchored(false)
                                wait(.3)
                                Teleport(CFrame.new(getgenv().Host.Character.HumanoidRootPart.Position + Vector3.new(DistanciaPrincipal, 0, 0)))
                                wait(1)
                                getgenv().WarpLoop = game:GetService("RunService").Heartbeat:Connect(function()
                                    Teleport2(CFrame.new(getgenv().Host.Character.HumanoidRootPart.Position + Vector3.new(DistanciaPrincipal, AlturaPrincipal, 0)))
                                    workspace.Gravity = 0
                                end)
                             elseif args[2] == 'head' then
                                 Anchored(false)
                                 wait(.3)
                                 Teleport(CFrame.new(getgenv().Host.Character.HumanoidRootPart.Position + Vector3.new(0, 5, 0)))
                                 wait(1)
                                 Teleport(CFrame.new(getgenv().Host.Character.HumanoidRootPart.Position + Vector3.new(0, Head, 0)))
                                 wait(0.3)
                                 Anchored(true)
                             elseif args[2] == 'base' or args[2] == 'adminbase' or args[2] == 'baseadmin' or args[2] == 'admin' then
                                Anchored(false)
                                wait(.4)
                                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-871.0387573242188, -38.39921188354492, -545.9679565429688)
                                 wait(1)
                                 local Adicional
 
                                 if a == 'Alt1' then
                                     Adicional = 3
                                 elseif a == 'Alt2' then
                                     Adicional = 6
                                 elseif a == 'Alt3' then
                                     Adicional = 9
                                 elseif a == 'Alt4' then
                                     Adicional = 12
                                 elseif a == 'Alt5' then
                                     Adicional = 15
                                 elseif a == 'Alt6' then
                                     Adicional = 18
                                 elseif a == 'Alt7' then
                                     Adicional = 21
                                 elseif a == 'Alt8' then
                                     Adicional = 24
                                 elseif a == 'Alt9' then
                                     Adicional = 27
                                 elseif a == 'Alt10' then
                                     Adicional = 30
                                 elseif a == 'Alt11' then
                                     Adicional = 33
                                 elseif a == 'Alt12' then
                                     Adicional = 36
                                 elseif a == 'Alt13' then
                                     Adicional = 39
                                 elseif a == 'Alt14' then
                                     Adicional = 42
                                 elseif a == 'Alt15' then
                                     Adicional = 45
                                 elseif a == 'Alt16' then
                                     Adicional = 48
                                 elseif a == 'Alt17' then
                                     Adicional = 51
                                 elseif a == 'Alt18' then
                                    Adicional = 54
                                 elseif a == 'Alt19' then
                                    Adicional = 57
                                 elseif a == 'Alt20' then
                                    Adicional = 60
                                 elseif a == 'Alt21' then
                                    Adicional = 63
                                 elseif a == 'Alt22' then
                                    Adicional = 66
                                 elseif a == 'Alt23' then
                                    Adicional = 69
                                 elseif a == 'Alt24' then
                                    Adicional = 72
                                 elseif a == 'Alt25' then
                                    Adicional = 75
                                 elseif a == 'Alt26' then
                                    Adicional = 78
                                 elseif a == 'Alt27' then
                                    Adicional = 81
                                 elseif a == 'Alt28' then
                                    Adicional = 84
                                 elseif a == 'Alt29' then
                                    Adicional = 87
                                 elseif a == 'Alt30' then
                                    Adicional = 90
                                 end
                                 wait()
                                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-871.0387573242188, -38.39921188354492, -545.9679565429688 - Adicional)
                                 wait(1)
                                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-871.0387573242188, -38.39921188354492 + 5, -545.9679565429688 - Adicional)
                                 wait(.1)
                                 Anchored(true)
                            elseif args[2] == 'safezone' then
                                Anchored(false)
                                wait(.4)
                                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(235.09283447265625, 38.25, 200021.453125)
                                 wait(1)
                                 local Adicional
 
                                 if a == 'Alt1' then
                                     Adicional = 3
                                 elseif a == 'Alt2' then
                                     Adicional = 6
                                 elseif a == 'Alt3' then
                                     Adicional = 9
                                 elseif a == 'Alt4' then
                                     Adicional = 12
                                 elseif a == 'Alt5' then
                                     Adicional = 15
                                 elseif a == 'Alt6' then
                                     Adicional = 18
                                 elseif a == 'Alt7' then
                                     Adicional = 21
                                 elseif a == 'Alt8' then
                                     Adicional = 24
                                 elseif a == 'Alt9' then
                                     Adicional = 27
                                 elseif a == 'Alt10' then
                                     Adicional = 30
                                 elseif a == 'Alt11' then
                                     Adicional = 33
                                 elseif a == 'Alt12' then
                                     Adicional = 36
                                 elseif a == 'Alt13' then
                                     Adicional = 39
                                 elseif a == 'Alt14' then
                                     Adicional = 42
                                 elseif a == 'Alt15' then
                                     Adicional = 45
                                 elseif a == 'Alt16' then
                                     Adicional = 48
                                 elseif a == 'Alt17' then
                                     Adicional = 51
                                 elseif a == 'Alt18' then
                                    Adicional = 54
                                 elseif a == 'Alt19' then
                                    Adicional = 57
                                 elseif a == 'Alt20' then
                                    Adicional = 60
                                 elseif a == 'Alt21' then
                                    Adicional = 63
                                 elseif a == 'Alt22' then
                                    Adicional = 66
                                 elseif a == 'Alt23' then
                                    Adicional = 69
                                 elseif a == 'Alt24' then
                                    Adicional = 72
                                 elseif a == 'Alt25' then
                                    Adicional = 75
                                 elseif a == 'Alt26' then
                                    Adicional = 78
                                 elseif a == 'Alt27' then
                                    Adicional = 81
                                 elseif a == 'Alt28' then
                                    Adicional = 84
                                 elseif a == 'Alt29' then
                                    Adicional = 87
                                 elseif a == 'Alt30' then
                                    Adicional = 90
                                 end
                                 wait()
                                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(235.09283447265625 - Adicional, 38.25, 200021.453125)
                                 wait(1)
                                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(235.09283447265625 - Adicional, 38.25 + 5, 200021.453125)
                                 wait(.1)
                                 Anchored(true)
                             end
                         end
     
                         if args[1] == Settings['Prefix'] .. 'gravity' then
                             getgenv().GravityOn = game:GetService("RunService").Heartbeat:Connect(function()
                                 workspace.Gravity = 0
                             end)
                         end
                         if args[1] == Settings['Prefix'] .. 'ungravity' then
                             if getgenv().GravityOn then
                                 getgenv().GravityOn:Disconnect()
                                 workspace.Gravity = getgenv().OldGravity
                             end
                         end
 
                         -- Chat
                         if args[1] == Settings['Prefix'] .. 'chat' then
                             Say(Settings['Chat'])
                         end
     
                         -- Bring alts
                         if args[1] == Settings['Prefix'] .. 'bring' then
                             Anchored(false)
                             wait(0.3)
                             Teleport(CFrame.new(getgenv().Host.Character.HumanoidRootPart.Position + Vector3.new(DistanciaPrincipal, AlturaPrincipal, 0)))
                         end
     
                         -- Drop
                         if args[1] == Settings['Prefix'] .. 'drop' then
                             Say('Start dropping')
                             getgenv().Drop = true
                             Block(true)
                             Wallet(true)
                             spawn(function()
                                 while getgenv().Drop == true do
                                    if game.Players.LocalPlayer.DataFolder.Currency.Value > 10000 then
                                        game:GetService("ReplicatedStorage").MainEvent:FireServer("DropMoney", 10000)
                                    else
                                        game:GetService("ReplicatedStorage").MainEvent:FireServer("DropMoney", game.Players.LocalPlayer.DataFolder.Currency.Value)
                                    end
                                    wait()
                                 end
                             end)
                         end
     
                         if args[1] == Settings['Prefix'] .. 'fps' then
                            setfpscap(args[2])
                            say('fps cap set to: '..args[2])
                         end
                         -- Stop
                         if args[1] == Settings['Prefix'] .. 'stop' then
                             getgenv().Drop = false
                             Block(false)
                             Wallet(false)
                             say('Stop dropping')
                         end
     
                         -- Airlock
                         if args[1] == Settings['Prefix'] .. 'airlock' then
                             if args[2] == 'true' then
                                 Anchored(false)
                                 wait(1)
                                 Teleport(CFrame.new(lCharacter.HumanoidRootPart.Position + Vector3.new(0, AlturaPrincipal, 0)))
                                 wait(0.3)
                                 Anchored(true)
                             elseif args[2] == 'false' then
                                 Anchored(false)
                             end
                         end
     
                         -- Hide
                         if args[1] == Settings['Prefix'] .. 'hide' then
                             Anchored(false)
                             wait(1)
                             Teleport(CFrame.new(0, 100, 0))
                             wait(0.3)
                             Anchored(true)
                         end
                    else
                        game.StarterGui:SetCore('SendNotification', {
                            Title = 'Sucess';
                            Text = 'Host found!';
                        })
                    end
                end)
            end
        end
    end
    
    
    
    function Restart()
        if game.Players.LocalPlayer.Name ~= Settings['Host'] then
            --Alt 1
            Alt(1, 2, 5, 10)
            --Alt 2
            Alt('2', 4, 5, 15)
            --Alt 3
            Alt('3', 6, 5, 20)
            --Alt 4
            Alt('4', 8, 5, 25)
            --Alt 5
            Alt('5', 10, 5, 30)
            --Alt 6
            Alt('6', 12, 5, 35)
            --Alt 7
            Alt('7', 14, 5, 40)
            --Alt 8
            Alt('8', 16, 5, 45)
            --Alt 9
            Alt('9', 18, 5, 50)
            --Alt 10
            Alt('10', 20, 5, 55)
            --ALt 11
            Alt('11', 2, 5, 60)
            --Alt 12
            Alt('12', 4, 5, 65)
            --Alt 13
            Alt('13', 6, 10, 70)
            --Alt 14
            Alt('14', 8, 10, 75)
            --Alt 15
            Alt('15', 10, 10, 80)
            --Alt 16
            Alt('16', 12, 10, 85)
            --Alt 17
            Alt('17', 14, 10, 90)
            --Alt 18
            Alt('18', 16, 10, 95)
            --Alt 19
            Alt('19', 18, 10, 100)
            --Alt 20
            Alt('20', 20, 10, 105)
            --Alt 21
