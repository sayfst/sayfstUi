 -- Auto buy
 Main:CreateTab('Auto buy')
 
 Main:CreateButton('Revolver', function()
     if Player.Backpack:FindFirstChild('[Revolver]') then
         Now = Character.HumanoidRootPart.Position
         for i = 1,100 do
             Teleport(CFrame.new(game:GetService("Workspace").Ignored.Shop["12 [Revolver Ammo] - $75"].Head.Position))
             fireclickdetector(game:GetService("Workspace").Ignored.Shop["12 [Revolver Ammo] - $75"].ClickDetector)
             Character.Humanoid.Sit = false
             wait()
         end
         Teleport(CFrame.new(Now))
     else
         CheckMoney(1300, function()
             Now = Character.HumanoidRootPart.Position
             repeat
                 Teleport(CFrame.new(game:GetService("Workspace").Ignored.Shop["[Revolver] - $1300"].Head.Position))
                 fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Revolver] - $1300"].ClickDetector)
                 Character.Humanoid.Sit = false
                 wait()
             until Player.Backpack:FindFirstChild('[Revolver]') ~= nil
             Teleport(CFrame.new(Now))
         end)
     end
 end)
 
