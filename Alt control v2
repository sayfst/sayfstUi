    getgenv().Host = nil
    if game.Players:FindFirstChild(Settings['Host']) then
        getgenv().Host = game.Players:FindFirstChild(Settings['Host'])
    end
    function Say(message)
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(message, 'All')
    end
    
    local Prefix = Settings['Prefix']
    
    local lPlayer = game.Players.LocalPlayer
    local lCharacter = lPlayer.Character
    lPlayer.CharacterAdded:Connect(function(Character)
        lCharacter = Character
    end)
    
   
    
    function Alt(Numero, Distancia, Altura, AlturaHead)
        local a = 'Alt'..Numero
        if game.Players.LocalPlayer.UserId == Settings[a] then
            if getgenv().Host ~= nil then
                delay(5, function()
                    setfpscap(getgenv().AltControlSettings['Fps'])
                    workspace:FindFirstChildOfClass('Terrain').WaterWaveSize=0;workspace:FindFirstChildOfClass('Terrain').WaterWaveSpeed=0;workspace:FindFirstChildOfClass('Terrain').WaterReflectance=0;workspace:FindFirstChildOfClass('Terrain').WaterTransparency=0;game:GetService("Lighting").GlobalShadows=false;game:GetService("Lighting").FogEnd=9e9;settings().Rendering.QualityLevel=1;for a,b in pairs(game:GetDescendants())do if b:IsA("Part")or b:IsA("UnionOperation")or b:IsA("MeshPart")or b:IsA("CornerWedgePart")or b:IsA("TrussPart")then b.Material="Plastic"b.Reflectance=0 elseif b:IsA("Decal")then b.Transparency=1 elseif b:IsA("ParticleEmitter")or b:IsA("Trail")then b.Lifetime=NumberRange.new(0)elseif b:IsA("Explosion")then b.BlastPressure=1;b.BlastRadius=1 end end;for a,b in pairs(game:GetService("Lighting"):GetDescendants())do if b:IsA("BlurEffect")or b:IsA("SunRaysEffect")or b:IsA("ColorCorrectionEffect")or b:IsA("BloomEffect")or b:IsA("DepthOfFieldEffect")then b.Enabled=false end end;workspace.DescendantAdded:Connect(function(c)coroutine.wrap(function()if c:IsA('ForceField')then game:GetService('RunService').Heartbeat:Wait()c:Destroy()elseif c:IsA('Sparkles')then game:GetService('RunService').Heartbeat:Wait()c:Destroy()elseif c:IsA('Smoke')or c:IsA('Fire')then game:GetService('RunService').Heartbeat:Wait()c:Destroy()end end)()end)
                end)
                getgenv().Host.Chatted:Connect(function(message)
                    if getgenv().Host ~= game.Players.LocalPlayer then
                        
                        function Teleport(Position)
                            game:GetService('TweenService'):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(0.3), {CFrame = Position}):Play()
                        end
                        function Teleport2(Position)
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Position
                        end
                        
                         function Anchored(state)
                             lCharacter.HumanoidRootPart.Anchored = state
                         end
                         function Drop(amonut)
                             game:GetService("ReplicatedStorage").MainEvent:FireServer('DropMoney', ''..amonut..'')
                         end
                         function Block(status)
                             local Target = game:GetService("ReplicatedStorage").MainEvent;
                             Target:FireServer("Block", status)
                         end
                         function Wallet(status)
                             if status == true then
                                 game.Players.LocalPlayer.Backpack:FindFirstChild("Wallet").Parent = game.Players.LocalPlayer.Character
                             elseif status == false then
                                 game.Players.LocalPlayer.Character:FindFirstChild("Wallet").Parent = game.Players.LocalPlayer.Backpack
                             end
                         end
                         args = message:split(' ')
                         argsSay = message:split(':')
                         if argsSay[1] == Settings['Prefix'] .. 'say' then
                             Say(argsSay[2])
                         end
     
                         -- Setup
                         local DistanciaPrincipal = Distancia + 5
                         local AlturaPrincipal = Altura + 2
                         local Head = AlturaHead + 2
                         getgenv().OldGravity = workspace.Gravity
                         if args[1] == Prefix .. 'warp' then
                             if args[2] == 'line' then
                                 Anchored(false)
                                 wait(.3)
                                 Teleport(CFrame.new(getgenv().Host.Character.HumanoidRootPart.Position + Vector3.new(DistanciaPrincipal, 0, 0)))
                                 wait(1)
                                 Teleport(CFrame.new(getgenv().Host.Character.HumanoidRootPart.Position + Vector3.new(DistanciaPrincipal, AlturaPrincipal, 0)))
                                 wait(0.3)
                                 Anchored(true)
                             elseif args[2] == 'disconnect' then
                                 Anchored(false)
                                 if getgenv().WarpLoop then
                                     
                                    getgenv().WarpLoop:Disconnect()
                                    workspace.Gravity = getgenv().OldGravity
                                 end
                             elseif args[2] == 'loop' then
                                Anchored(false)
                                wait(.3)
                                Teleport(CFrame.new(getgenv().Host.Character.HumanoidRootPart.Position + Vector3.new(DistanciaPrincipal, 0, 0)))
                                wait(1)
                                getgenv().WarpLoop = game:GetService("RunService").Heartbeat:Connect(function()
                                    Teleport2(CFrame.new(getgenv().Host.Character.HumanoidRootPart.Position + Vector3.new(DistanciaPrincipal, AlturaPrincipal, 0)))
                                    workspace.Gravity = 0
                                end)
                             elseif args[2] == 'head' then
                                 Anchored(false)
                                 wait(.3)
                                 Teleport(CFrame.new(getgenv().Host.Character.HumanoidRootPart.Position + Vector3.new(0, 5, 0)))
                                 wait(1)
                                 Teleport(CFrame.new(getgenv().Host.Character.HumanoidRootPart.Position + Vector3.new(0, Head, 0)))
                                 wait(0.3)
                                 Anchored(true)
                             elseif args[2] == 'base' or args[2] == 'adminbase' or args[2] == 'baseadmin' or args[2] == 'admin' then
                                Anchored(false)
                                wait(.4)
                                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-871.0387573242188, -38.39921188354492, -545.9679565429688)
                                 wait(1)
                                 local Adicional
 
                                 if a == 'Alt1' then
                                     Adicional = 3
                                 elseif a == 'Alt2' then
                                     Adicional = 6
                                 elseif a == 'Alt3' then
                                     Adicional = 9
                                 elseif a == 'Alt4' then
                                     Adicional = 12
                                 elseif a == 'Alt5' then
                                     Adicional = 15
                                 elseif a == 'Alt6' then
                                     Adicional = 18
                                 elseif a == 'Alt7' then
                                     Adicional = 21
                                 elseif a == 'Alt8' then
                                     Adicional = 24
                                 elseif a == 'Alt9' then
                                     Adicional = 27
