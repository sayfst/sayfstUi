                        -- Airlock
                        if args[1] == Settings['Prefix'] .. 'airlock' then
                            if args[2] == 'true' then
                                Anchored(false)
                                wait(1)
                                Teleport(CFrame.new(lCharacter.HumanoidRootPart.Position + Vector3.new(0, AlturaPrincipal, 0)))
                                wait(0.3)
                                Anchored(false)
                            elseif args[2] == 'false' then
                                Anchored(false)
                            end
                        end

