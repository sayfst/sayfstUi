            Alt('26', 12, 15, 135)
            --Alt 27
            Alt('27', 14, 15, 140)
            --Alt 28
            Alt('28', 16, 15, 145)
            --Alt 29
            Alt('29', 18, 15, 150)
            --Alt 30
            Alt('30', 20, 15, 155)
        end
    end
    
    Restart()
    game.Players.ChildAdded:Connect(function(Player)
        if Player.Name == Settings['Host'] then
            getgenv().Host = Player
            Restart()
        end
    end) 
