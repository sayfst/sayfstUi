                                    Adicional = 63
                                 elseif a == 'Alt22' then
                                    Adicional = 66
                                 elseif a == 'Alt23' then
                                    Adicional = 69
                                 elseif a == 'Alt24' then
                                    Adicional = 72
                                 elseif a == 'Alt25' then
                                    Adicional = 75
                                 elseif a == 'Alt26' then
                                    Adicional = 78
                                 elseif a == 'Alt27' then
                                    Adicional = 81
                                 elseif a == 'Alt28' then
                                    Adicional = 84
                                 elseif a == 'Alt29' then
                                    Adicional = 87
                                 elseif a == 'Alt30' then
                                    Adicional = 90
                                 end
                                 wait()
                                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(235.09283447265625 - Adicional, 38.25, 200021.453125)
                                 wait(1)
                                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(235.09283447265625 - Adicional, 38.25 + 5, 200021.453125)
                                 wait(.1)
                                 Anchored(true)
                             end
                         end
     
                         if args[1] == Settings['Prefix'] .. 'gravity' then
                             getgenv().GravityOn = game:GetService("RunService").Heartbeat:Connect(function()
                                 workspace.Gravity = 0
                             end)
                         end
                         if args[1] == Settings['Prefix'] .. 'ungravity' then
                             if getgenv().GravityOn then
                                 getgenv().GravityOn:Disconnect()
                                 workspace.Gravity = getgenv().OldGravity
                             end
                         end
 
                         -- Chat
                         if args[1] == Settings['Prefix'] .. 'chat' then
                             Say(Settings['Chat'])
                         end
     
                         -- Bring alts
                         if args[1] == Settings['Prefix'] .. 'bring' then
                             Anchored(false)
                             wait(0.3)
                             Teleport(CFrame.new(getgenv().Host.Character.HumanoidRootPart.Position + Vector3.new(DistanciaPrincipal, AlturaPrincipal, 0)))
                         end
     
                         -- Drop
                         if args[1] == Settings['Prefix'] .. 'drop' then
                             Say('Start dropping')
                             getgenv().Drop = true
                             Block(true)
                             Wallet(true)
                             spawn(function()
                                 while getgenv().Drop == true do
                                    if game.Players.LocalPlayer.DataFolder.Currency.Value > 10000 then
                                        game:GetService("ReplicatedStorage").MainEvent:FireServer("DropMoney", 10000)
                                    else
                                        game:GetService("ReplicatedStorage").MainEvent:FireServer("DropMoney", game.Players.LocalPlayer.DataFolder.Currency.Value)
                                    end
                                    wait()
                                 end
                             end)
                         end
     
                         if args[1] == Settings['Prefix'] .. 'fps' then
                            setfpscap(args[2])
                            say('fps cap set to: '..args[2])
                         end
                         -- Stop
                         if args[1] == Settings['Prefix'] .. 'stop' then
                             getgenv().Drop = false
                             Block(false)
                             Wallet(false)
                             say('Stop dropping')
                         end
     
                         -- Airlock
                         if args[1] == Settings['Prefix'] .. 'airlock' then
                             if args[2] == 'true' then
                                 Anchored(false)
                                 wait(1)
                                 Teleport(CFrame.new(lCharacter.HumanoidRootPart.Position + Vector3.new(0, AlturaPrincipal, 0)))
                                 wait(0.3)
                                 Anchored(true)
                             elseif args[2] == 'false' then
                                 Anchored(false)
                             end
                         end
     
                         -- Hide
                         if args[1] == Settings['Prefix'] .. 'hide' then
                             Anchored(false)
                             wait(1)
                             Teleport(CFrame.new(0, 100, 0))
                             wait(0.3)
                             Anchored(true)
                         end
                    else
                        game.StarterGui:SetCore('SendNotification', {
                            Title = 'Sucess';
                            Text = 'Host found!';
                        })
                    end
                end)
            end
        end
    end
    
    
    
    function Restart()
        if game.Players.LocalPlayer.Name ~= Settings['Host'] then
            --Alt 1
            Alt(1, 2, 5, 10)
            --Alt 2
            Alt('2', 4, 5, 15)
