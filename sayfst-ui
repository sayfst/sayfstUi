TextBox.Size = UDim2.new(0, 249, 0, 32)
TextBox.Font = Enum.Font.SourceSans
TextBox.Text = "Player"
TextBox.TextColor3 = Color3.fromRGB(0, 0, 0)
TextBox.TextSize = 14.000

-- Scripts:

local function QXULJTT_fake_script() -- TextBox.LocalScript 
	local script = Instance.new('LocalScript', TextBox)

	if not game:IsLoaded() then
		game.Loaded:wait()
	end
	
	script.Parent.Parent:TweenPosition(UDim2.new(0.5, 0,0.095, 0), "Out", "Quart", 1, true)
	script.Parent:CaptureFocus()
	
	script.Parent.FocusLost:Connect(function(a)
		if a then
			for i,v in pairs(game.Players:GetChildren()) do
				if string.lower(script.Parent.Text) == string.lower(v.Name) or string.lower(script.Parent.Text) == string.lower(v.DisplayName) then
					if v.Character then
						script.Parent.Parent:TweenPosition(UDim2.new(0.5, 0,-1.095, 0), "Out", "Quart", 1, true)
						delay(1, function()
							script.Parent.Parent.Parent:Destroy()
						end)
						getgenv().Player = script.Parent.Text
	
						local function SendNotification(title, text)
							game.StarterGui:SetCore("SendNotification", {
								Title = title;
								Text = text;
							})
						end
	
						local function undoHumanoid()
							if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
								game.Players.LocalPlayer.Character.Humanoid.Name = 1
								local clone = game.Players.LocalPlayer.Character["1"]:Clone()
								game.Players.LocalPlayer.Character["1"]:Destroy()
								wait()
								clone.Parent = game.Players.LocalPlayer.Character
								clone.Name = "Humanoid"
							end
						end
	
						SendNotification("Notification", "finding  the player")
						if game:GetService("Players"):FindFirstChild(getgenv().Player) then
							SendNotification("Notification", "player found") wait(.1)
							SendNotification("Notification", "checking if you have a tool equipped")
							for a,b in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
								if b:IsA("Tool") then
									SendNotification("Notification", "tool found") wait(.1)
									getgenv().Tool = b
									SendNotification("Notification", "unequipping " .. b.Name )
									b.Parent = game.Players.LocalPlayer.Backpack
									local now = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
									for i = 1,5 do
										game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players:FindFirstChild(getgenv().Player).Character.HumanoidRootPart.CFrame
										wait(.1)
									end
									undoHumanoid()
									delay(.5, function()
										getgenv().Tool.Parent = game.Players.LocalPlayer.Character
									end)
									repeat 
										game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players:FindFirstChild(getgenv().Player).Character.HumanoidRootPart.CFrame
										wait()
									until getgenv().Tool.Parent == game.Players:FindFirstChild(getgenv().Player).Character 
									for i = 1,100 do
										game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = now
										wait()
									end
								else
									SendNotification("Error, Try again", "You dont have a tool with handle equipped!")
								end
							end
						end
					end
				end
			end
		end
	end)
end
coroutine.wrap(QXULJTT_fake_script)()

end)
function SendNotification(title, text)
	game.StarterGui:SetCore("SendNotification", {
		Title = title;
		Text = text;
	})
end

