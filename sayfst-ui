local ver = "v0.0.4"
TextLabel_2.Text = "Version: "..ver
loadstring(game:HttpGet('https://raw.githubusercontent.com/sayfst/sayfstUi/main/sayfst-version'))()
if ver == getgenv().Version1 then
    SendNotification("Version", "You are on the most up-to-date version")
else
    SendNotification("Version", 'New update found, Send in chat "updatesayfst" To update sayfst')
end

function ResetAutoUpdate()
    wait(1)
    game.Players.LocalPlayer.chatted:Connect(function(message)
        if message == "updatesayfst" then
            loadstring(game:HttpGet('https://raw.githubusercontent.com/sayfst/sayfstUi/main/sayfst-version'))()
            if not ver == getgenv().Version1 then
                getgenv().Frame:TweenPosition(UDim2.new(1.751280189, 0, 0.238596484, 0), "InOut", "Quart", 1, true)
                wait(1)
                SendNotification("Version","Updating...")
                wait(1)
                ScreenGui:Destroy()
                loadstring(game:HttpGet('https://raw.githubusercontent.com/sayfst/sayfstUi/main/sayfst-ui'))()
            else
                SendNotification("Error", "You already in the lasted version")
            end
        end
    end)
end


ResetAutoUpdate()
game.Players.LocalPlayer.CharacterAdded:Connect(function()
    ResetAutoUpdate()
end)


