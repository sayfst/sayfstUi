Frame.Draggable = true

getgenv().Frame = Frame
wait()
getgenv().Frame:TweenPosition(UDim2.new(0.751280189, 0, 0.238596484, 0), "Out", "Quart", 1, true)

UICorner.Parent = Frame

function CreateDropDown(dropname, playerDrop, callback)
    local callback = callback or function()end
    local ScreenGui = Instance.new("ScreenGui")
    local Frame = Instance.new("Frame")
    local Frame_2 = Instance.new("Frame")
    local UICorner = Instance.new("UICorner")
    local TextLabel = Instance.new("TextLabel")
    local TextBox = Instance.new("TextBox")
    local UICorner_2 = Instance.new("UICorner")
    local UICorner_3 = Instance.new("UICorner")
    
    
    ScreenGui.Parent = game.CoreGui
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    
    Frame.Parent = ScreenGui
    Frame.AnchorPoint = Vector2.new(0.5, 0.5)
    Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Frame.Position = UDim2.new(0.5, 0, -1.16799998, 0)
    Frame.Size = UDim2.new(0, 442, 0, 141)
    
    Frame_2.Parent = Frame
    Frame_2.AnchorPoint = Vector2.new(0.5, 0.5)
    Frame_2.BackgroundColor3 = Color3.fromRGB(38, 38, 38)
    Frame_2.Position = UDim2.new(0.499634236, 0, 0.498363405, 0)
    Frame_2.Size = UDim2.new(0, 440, 0, 139)
    
    UICorner.Parent = Frame_2
    
    TextLabel.Parent = Frame_2
    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel.BackgroundTransparency = 1.000
    TextLabel.Size = UDim2.new(0, 440, 0, 30)
    TextLabel.Font = Enum.Font.SourceSansBold
    TextLabel.Text = "Sayfst "..dropname
    TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel.TextSize = 35.000
    TextLabel.TextWrapped = true
    
    TextBox.Parent = Frame_2
    TextBox.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    TextBox.Position = UDim2.new(0.272727281, 0, 0.467625886, 0)
    TextBox.Size = UDim2.new(0, 200, 0, 50)
    TextBox.Font = Enum.Font.SourceSans
    TextBox.Text = "Player"
    TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextBox.TextSize = 41.000
    
    UICorner_2.Parent = TextBox
    
    UICorner_3.Parent = Frame

    local function NNUEUDI_fake_script() -- TextBox.LocalScript 
        local script = Instance.new('LocalScript', TextBox)
    
        script.Parent.Parent.Parent.Parent.Frame:TweenPosition(UDim2.new(0.5, 0,0.168, 0), "Out", "Quart", 1, true)
        
        script.Parent:CaptureFocus()
        
        script.Parent.FocusLost:Connect(function(a)
            if a then
                if game.Players:FindFirstChild(script.Parent.Text) then
                    script.Parent.Parent.Parent.Parent.Frame:TweenPosition(UDim2.new(0.5, 0,-1.168, 0), "Out", "Quart", 1, true)
                    playerDrop = game.Players:FindFirstChild(script.Parent.Text)
                    pcall(callback, playerDrop)
                    script.Parent.Parent.Parent.Parent.Frame:TweenPosition(UDim2.new(0.5, 0,-1.168, 0), "Out", "Quart", 1, true)
                else
                    script.Parent.Text = "Error, Player nao encontrado"
                    wait(1)
                    script.Parent:CaptureFocus()
                end
            end
        end)
    end
    coroutine.wrap(NNUEUDI_fake_script)()
    
end

