    -- Scripts:
    
    local function ANLI_fake_script() -- CREDITS.LocalScript 
        local script = Instance.new('LocalScript', CREDITS)
    
        script.Parent.MouseButton1Click:Connect(function()
            game.StarterGui:SetCore('SendNotification', {
                Title = 'Credits';
                Text = 'MADE BY HENRIQUE';
                Duration = 5;
                Button1 = 'Ok'
            })
        end)
    end
    coroutine.wrap(ANLI_fake_script)()
    local function TZOU_fake_script() -- PLAY.SCRIPT 
        local script = Instance.new('LocalScript', PLAY)
    
        script.Parent--[[]].MouseButton1Click:Connect(function()
            print(workspace.Baseplate.Color)
        end)
    end
    coroutine.wrap(TZOU_fake_script)()
