    local function ZLRCK_fake_script() -- Config.LocalScript 
        local script = Instance.new('LocalScript', Config)
    
        script.Parent.MouseButton1Click:Connect(function()
            script.Parent.Parent.Parent["MENU PRINCIPAL"].Visible = false
            script.Parent.Parent.Parent["MENU Config"].Visible = true
        end)
    end
