    local tabCreator = {}

    function tabCreator:CreateTab(title)
        local TextButton = Instance.new("TextButton")
        local Tab = Instance.new("ScrollingFrame")
        local UIListLayout = Instance.new("UIListLayout")


        TextButton.Parent = BotoesTab
        TextButton.BackgroundColor3 = Color3.fromRGB(31, 31, 31)
        TextButton.BorderSizePixel = 0
        TextButton.Size = UDim2.new(0, 116, 0, 29)
        TextButton.Font = Enum.Font.RobotoMono
        TextButton.Text = title
        TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextButton.TextSize = 14.000
        TextButton.MouseButton1Click:Connect(function()
            for i,v in pairs(TUDO:GetChildren()) do
                if v:IsA('ScrollingFrame') and v.Name ~= 'BotoesTab' then
                    v.Visible = false
                end
            end
            Tab.Visible = true
        end)

        

        Tab.Name = "Tab"
        Tab.Parent = TUDO
        Tab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Tab.BackgroundTransparency = 1.000
        Tab.BorderSizePixel = 0
        Tab.Position = UDim2.new(0.270377725, 0, 0, 0)
        Tab.Size = UDim2.new(0, 365, 0, 389)
        Tab.Visible = false
        Tab.AutomaticSize = Enum.AutomaticSize.Y

        UIListLayout.Parent = Tab
        UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
        UIListLayout.Padding = UDim.new(0, 5)

        local buttons = {}

        function buttons:CreateButton(title,callback)
            local Button = Instance.new("TextButton")
            local UICorner = Instance.new("UICorner")



            Button.Name = title
            Button.Parent = Tab
            Button.BackgroundColor3 = Color3.fromRGB(42, 42, 42)
            Button.BorderSizePixel = 0
            Button.Size = UDim2.new(0, 357, 0, 15)
            Button.BackgroundTransparency = 1
            Button.Font = Enum.Font.RobotoCondensed
            Button.Text = title
            Button.TextColor3 = Color3.fromRGB(255, 255, 255)
            Button.TextSize = 14.000
            Button.MouseButton1Click:Connect(function()
                pcall(callback, Button)
            end)
            task.spawn(function()
                function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
        
                counter = 0
                
                while wait(0.1)do
                    Button.TextColor3 = Color3.fromHSV(zigzag(counter),1,1)
                
                    counter = counter + 0.01
                end
            end)
            

            UICorner.Parent = Button
        end
        function buttons:CreateToggle(title, callback)
            local Toggle = Instance.new("TextButton")
            local UICorner = Instance.new("UICorner")
            local Fundo = Instance.new("Frame")
            local UICorner_2 = Instance.new("UICorner")
            local Bola = Instance.new("Frame")
            local UICorner_3 = Instance.new("UICorner")

            task.spawn(function()
                function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
        
                counter = 0
                
                while wait(0.1)do
                    Toggle.TextColor3 = Color3.fromHSV(zigzag(counter),1,1)
                
                    counter = counter + 0.01
                end
            end)

            Toggle.Name = "Toggle"
            Toggle.Parent = Tab
            Toggle.BackgroundColor3 = Color3.fromRGB(42, 42, 42)
            Toggle.BackgroundTransparency = 1
            Toggle.BorderSizePixel = 0
            Toggle.Size = UDim2.new(0, 357, 0, 15)
            Toggle.Font = Enum.Font.RobotoCondensed
            Toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
            Toggle.Text = title
            Toggle.TextSize = 14.000

            UICorner.Parent = Toggle

            Fundo.Name = "Fundo"
            Fundo.Parent = Toggle
            Fundo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Fundo.Position = UDim2.new(0.823529422, 0, 0.241379306, 0)
            Fundo.Size = UDim2.new(0, 34, 0, 14)

            UICorner_2.Parent = Fundo

            Bola.Name = "Bola"
            Bola.Parent = Fundo
            Bola.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            Bola.Position = UDim2.new(0, 5, 0.143000007, 0)
            Bola.Size = UDim2.new(0, 10, 0, 10)

            UICorner_3.Parent = Bola

            
            pcall(function()
                local toggled = false
                local tw = TweenInfo.new(0.5, Enum.EasingStyle.Quart)
                local ts = game:GetService('TweenService')
                Toggle.MouseButton1Click:Connect(function()
                    toggled = not toggled
                    if toggled == true then
                        ts:Create(Fundo, tw, {BackgroundColor3 = Color3.fromRGB(98, 101, 255)}):Play()
                        ts:Create(Bola, tw, {BackgroundColor3 = Color3.fromRGB(255,255,255)}):Play()
                        ts:Create(Bola, tw, {Position = UDim2.new(0.55, 0,0.143, 0)}):Play()
                    else
                        ts:Create(Bola, tw, {BackgroundColor3 = Color3.fromRGB(0, 0, 0)}):Play()
                        ts:Create(Fundo, tw, {BackgroundColor3 = Color3.fromRGB(255, 255, 255)}):Play()
                        ts:Create(Bola, tw, {Position = UDim2.new(0, 5,0.143, 0)}):Play()
                    end
                    task.spawn(function()
                        pcall(callback, toggled)
                    end)
                end)
            end)
        end
        function buttons:CreateLabel(text)
            local Label = Instance.new("TextLabel")
            local UICorner = Instance.new("UICorner")
            
            
            
            Label.Name = "Label"
            Label.Parent = Tab
            Label.Active = true
            Label.BackgroundColor3 = Color3.fromRGB(56, 56, 56)
            Label.BorderSizePixel = 0
            Label.Position = UDim2.new(0, 0, 0.174807191, 0)
            Label.Selectable = true
            Label.BackgroundTransparency = 1
            Label.Size = UDim2.new(0, 357, 0, 22)
            Label.Font = Enum.Font.RobotoCondensed
            Label.Text = text
            Label.TextColor3 = Color3.fromRGB(255, 255, 255)
            Label.TextSize = 14.000
            task.spawn(function()
                function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
        
                counter = 0
                
                while wait(0.1)do
                    Label.TextColor3 = Color3.fromHSV(zigzag(counter),1,1)
                
                    counter = counter + 0.01
                end
            end)
            UICorner.Parent = Label
            return Label;
        end
        return buttons
    end
    return tabCreator
end

return lib
