local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/sayfst/sayfstUi/main/kavo%20library"))()

local Window = Library.CreateLib("3008 [2.7] script", "Serpent")
 
local Main = Window:NewTab("Freatures")

local DestroyBase = Window:NewTab('DESTROYER')

local DestroySection = DestroyBase:NewSection('Base destroyer')

local MainSection = Main:NewSection("Food")

local LocalPlayer = Main:NewSection('Local Player')

local BaseItems = Main:NewSection('Base Items')



local lplr = game:GetService('Players').LocalPlayer



local function FindPlayer(String)
    for _, _Player in pairs(game.Players:GetPlayers()) do
        if _Player.Name:lower():match('^'.. String:lower()) then
            return _Player
        end
    end
    return nil
end

MainSection:NewButton('Fill inventory', 'he will fill his inventory with food', function()
    local oldCFrame = lplr.Character.HumanoidRootPart.CFrame
    local oldPosition = lplr.Character.HumanoidRootPart.Position
    
    local ItemOnBackpack = 0
    game.RunService.RenderStepped:Connect(function()
        ItemOnBackpack = 0
        for i,v in pairs(lplr.Backpack:GetChildren()) do
            ItemOnBackpack = ItemOnBackpack + 1
        end
    end)
    
    for i,v in pairs(game:GetService("Workspace").GameObjects.Physical.Items:GetChildren()) do
        if v ~= nil then
            if v:FindFirstChild('Root') and 
            ItemOnBackpack <= 15 and 
            (v.Root.Position - oldPosition).Magnitude >= 25 then
                lplr.Character.HumanoidRootPart.CFrame = v.Root.CFrame
                wait()
                local args = {
                    [1] = {
                        ["Action"] = "AlternateInteract",
                        ["Model"] = v,
                    }
                }
                
                game:GetService("Players").LocalPlayer.Character.server_PickupSystem.MainEvent:InvokeServer(unpack(args))
                game:GetService("Players").LocalPlayer.Character.server_PickupSystem.MainEvent:InvokeServer(unpack(args))
                
                wait()
            end
        end
    end
    for i,v in pairs(game:GetService("Workspace").GameObjects.Physical.Map.Ground:GetChildren()) do
        if v ~= nil then
            if v:FindFirstChild('Root') ~= nil and
            ItemOnBackpack <= 15 and
            (v.Root.Position - oldPosition).Magnitude >= 25 then
                lplr.Character.HumanoidRootPart.CFrame = v.Root.CFrame
                wait()
                local args = {
                    [1] = {
                        ["Action"] = "AlternateInteract",
                        ["Model"] = v,
                    }
                }
                
                game:GetService("Players").LocalPlayer.Character.server_PickupSystem.MainEvent:InvokeServer(unpack(args))
                game:GetService("Players").LocalPlayer.Character.server_PickupSystem.MainEvent:InvokeServer(unpack(args))
                
                wait()
            end
        end
    end

    lplr.Character.HumanoidRootPart.CFrame = oldCFrame
end)

MainSection:NewButton('Med kits', nil, function()
    local ItemOnBackpack = 0
    game.RunService.RenderStepped:Connect(function()
        ItemOnBackpack = 0
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            ItemOnBackpack = ItemOnBackpack + 1
        end
    end)
    local lplr = game.Players.LocalPlayer
    local oldCFrame = lplr.Character.HumanoidRootPart.CFrame
    local oldPosition = lplr.Character.HumanoidRootPart.Position
    for i,v in pairs(game:GetService("Workspace").GameObjects.Physical.Items:GetChildren()) do
        if v.Name == 'Medkit' then
            local TargetMedKit;
            if v:FindFirstChild('Medkit') ~= nil then
                TargetMedKit = v:FindFirstChild('Medkit')
            elseif v:IsA('Model') then
                TargetMedKit = nil
            elseif TargetMedKit:IsA('MeshPart') then
                TargetMedKit = v;
            end
            if TargetMedKit ~= nil and (TargetMedKit.Position - oldPosition).Magnitude >=25  and ItemOnBackpack ~= 16 then
                lplr.Character.HumanoidRootPart.CFrame = CFrame.new(TargetMedKit.Position)
                local args = {
                    [1] = {
                        ["Action"] = "AlternateInteract",
                        ["Model"] = TargetMedKit.Parent,
                    }
                }
                
                game:GetService("Players").LocalPlayer.Character.server_PickupSystem.MainEvent:InvokeServer(unpack(args))
                game:GetService("Players").LocalPlayer.Character.server_PickupSystem.MainEvent:InvokeServer(unpack(args))
                    
                wait()
            end
        end
    end
    wait()
    repeat
        lplr.Character.HumanoidRootPart.CFrame = oldCFrame
        wait()
    until (lplr.Character.HumanoidRootPart.Position - oldPosition).Magnitude <=15

end)

MainSection:NewButton('Drop foods', nil, function()
    for i = 1,5 do
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do

            if v:IsA('Tool') then
    
                    v.Parent = game.Players.LocalPlayer.Character
    
            end
    
    end
    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
            if v:IsA('Tool') then
                    local args = {
                        [1] = {
                            ["Action"] = "DropStack",
                            ["Model"] = game:GetService("Players").LocalPlayer.Character[v.Name]
                        }
                    }
                    
                    game:GetService("Players").LocalPlayer.Character.server_PickupSystem.Inventory:InvokeServer(unpack(args))
            end
    end
    end
end)

MainSection:NewButton('Esp foods', 'he will create a esp on foods', function()
    for i,v in pairs(game:GetService("Workspace").GameObjects.Physical.Items:GetChildren()) do
        if v ~= nil then
            if v:FindFirstChild('Root') then
                local BillboardGui = Instance.new('BillboardGui') -- Makes Billboardgui
                local TextLabel = Instance.new('TextLabel') -- makes text label

                BillboardGui.Parent = v.Root -- what the billboardgui goes into
                BillboardGui.AlwaysOnTop = true -- if its on top or not
                BillboardGui.Size = UDim2.new(0, 50, 0, 100) -- size of it
                BillboardGui.StudsOffset = Vector3.new(0,2,0)

                TextLabel.Parent = BillboardGui -- putting textlabel into billboardgui
                TextLabel.BackgroundColor3 = Color3.new(1,1,1) -- color
                TextLabel.BackgroundTransparency = 1 -- transparency
                TextLabel.Size = UDim2.new(1, 0, 1, 0) -- size
                TextLabel.Text = v.Name
                TextLabel.TextColor3 = Color3.fromRGB(255, 0, 0)

            end
        end
    end
end)


