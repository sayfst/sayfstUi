BaseSection:NewButton('Auto build box - Player', nil, function()
	if _G.PlayerToBase ~= nil then
		local Plrr = _G.PlayerToBase
		local Camera = workspace.CurrentCamera
		local POSBASE = Plrr.Character.HumanoidRootPart.Position
		local Esquerda = POSBASE + Vector3.new(5, 0, 0)
		local Direita = POSBASE + Vector3.new(-5, 0, 0)
		local Frent = POSBASE + Vector3.new(0, 0, -5)
		local Traz = POSBASE + Vector3.new(0, 0, 5)
		local Cima = POSBASE + Vector3.new(0, 5, 0)
	
		_G.DistanceItem = 50
		_G.VezesLight = 1
		NewWood()
		clickKey(0x11)
		clickKey(0x10)
		clickKey(0x52)
		for i = 1,5 do
	
			keypress(0x34)
			
			task.wait()
			
			keyrelease(0x34)
	
			task.wait()
		end
		for i = 1, _G.DistanciaItem do
			clickKey(0x35)
			task.wait(.1)
		end
		Plrr.Character.HumanoidRootPart.CFrame = CFrame.new(POSBASE)
		wait()
		Camera.CFrame = CFrame.new(Camera.CFrame.Position, Esquerda)
		clickKey(0x33)
		wait()
		clickKey(0x52)
		wait(.2)
		clickKey(0x52)
		wait(.5)
		ApertarE()
		wait()
		NewWood()
	
		Plrr.Character.HumanoidRootPart.CFrame = CFrame.new(POSBASE)
		wait()
		Camera.CFrame = CFrame.new(Camera.CFrame.Position, Direita)
		wait(.2)
		ApertarE()
		wait()
		NewWood()
	
		Plrr.Character.HumanoidRootPart.CFrame = CFrame.new(POSBASE)
		wait()
		Camera.CFrame = CFrame.new(Camera.CFrame.Position, Frent)
		wait(.2)
		clickKey(0x31)
		wait()
		clickKey(0x52)
		wait(.2)
		clickKey(0x52)
		wait(.5)
		ApertarE()
		wait()
		NewWood()
	
		Plrr.Character.HumanoidRootPart.CFrame = CFrame.new(POSBASE)
		wait()
		Camera.CFrame = CFrame.new(Camera.CFrame.Position, Traz)
		wait(.2)
		ApertarE()
		wait()
		NewWood()
	
		Plrr.Character.HumanoidRootPart.CFrame = CFrame.new(POSBASE)
		wait()
		Camera.CFrame = CFrame.new(Camera.CFrame.Position, Cima)
		clickKey(0x11)
		clickKey(0x10)
		clickKey(0x52)
		wait(.5)
		ApertarE()
		wait()
	end
end)

