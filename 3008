		PickupLight(string[2], _G.VezesLight, string[3])
	end)
end)

if (writefile and readfile) then
	BaseItems:NewTextBox('Save Pickup option', '1 = Texto, 2 = Item, 3 = Root, 4 = Arquivo name', function(string)
		string = string:split(',')
		local Configuracoes = {
			Text = string[1];
			Item = string[2];
			Root = string[3];
			Nome = string[4];
		}
		local HttpService = game:GetService('HttpService')
		

		local nomeArquivo = string[4]..'.txt'


		local json = HttpService:JSONEncode(Configuracoes)

		writefile(nomeArquivo, json)
	end)
	BaseItems:NewTextBox('Load pickup option', nil, function(string)
		string = string..'.txt'
		local arquivo = game:GetService('HttpService'):JSONDecode(readfile(string))

		local HttpService = game:GetService('HttpService')
		local json = HttpService:JSONDecode(readfile(string))

		BaseItems:NewButton(json.Text,nil,function()
			PickupLight(json.Item,_G.VezesLight,json.Root)
		end)
	end)
end	

game.Players.LocalPlayer.Chatted:Connect(function(msg)
    if msg == '/e checkname' then
        local mouse = game.Players.LocalPlayer:GetMouse()

        local target = mouse.Target

        if target ~= nil and
        target.Parent:IsA('Model') then
            local BillboardGui = Instance.new('BillboardGui') -- Makes Billboardgui
            local TextLabel = Instance.new('TextLabel') -- makes text label
            setclipboard(target.Parent.Name..','..target.Name)
            BillboardGui.Parent = target -- what the billboardgui goes into
            BillboardGui.AlwaysOnTop = true -- if its on top or not
            BillboardGui.Size = UDim2.new(0, 50, 0, 100) -- size of it
            BillboardGui.StudsOffset = Vector3.new(0,2,0)

            TextLabel.Parent = BillboardGui -- putting textlabel into billboardgui
            TextLabel.BackgroundColor3 = Color3.new(1,1,1) -- color
            TextLabel.BackgroundTransparency = 1 -- transparency
			TextLabel.TextSize = 40
            TextLabel.Size = UDim2.new(1, 0, 1, 0) -- size
            TextLabel.Text = target.Parent.Name..'/'..target.Name
            TextLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
            wait(2)
            BillboardGui:Destroy()
        end
    end
end)


local plrDestroy = nil
local DestroyMagnitude = 50
DestroySection:NewTextBox('Player to base destroy',nil,function(string)
	for i,v in pairs(game.Players:GetChildren()) do
		if v.Name:lower():match('^'..string:lower()) or v.DisplayName:lower():match('^'..string:lower())  and v ~= game.Players.LocalPlayer then
			plrDestroy = v;
			game.StarterGui:SetCore('SendNotification', {
				Title = 'Sucess select plr';
				Text = 'plr: '..v.Name
			})
		end
	end
end)

DestroySection:NewTextBox('Distance', 'Distance from the items you will destroy', function(string)
	DestroyMagnitude = tonumber(string)
	game.StarterGui:SetCore('SendNotification', {
		Title = 'Sucess select distance';
		Text = 'Distance set to '..string
	})
end)

function clickKey(key)
    keypress(key)
    delay(0.1, function()
        task.wait()
        keyrelease(key)
    end)
end

DestroySection:NewButton('Destroy base', nil, function()
	local TextStatus = CreateStatusGui()
	TextStatus.Text = 'Starting on 1 seconds...'
	_G.DestroyRunning = true
	_G.ItemsPego = 0
	_G.FinalDestroy = false
	noclip2(true)
	wait(1) -- esse e o script do destroy base
	local oldposDESTROY = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
	
	local plrPOSITION = plrDestroy.Character.HumanoidRootPart.Position
	for i,v in pairs(game:GetService("Workspace").GameObjects.Physical.Map.Ground:GetDescendants()) do
        if v:FindFirstChild('Root') ~= nil or v:FindFirstChild('Part') ~= nil or v:FindFirstChild('Main') and
		plrDestroy.Character ~= nil then
			local Root = v:FindFirstChild('Root') or v:FindFirstChild('Part') or v:FindFirstChild('Main')
			
			if Root:IsA('Part') or Root:IsA('MeshPart') then
				if (plrPOSITION - Vector3.new(Root.Position.X, Root.Position.Y, Root.Position.Z)).Magnitude < DestroyMagnitude then
					if _G.DestroyRunning == true then
						local oldPOSROOT = Root.Position
						local vezes = 0
						TextStatus.Text = 'Items already taken: '.._G.ItemsPego..'\n'..'Pickuping: '..Root.Parent.Name
						repeat 
							vezes = vezes + 1
							
							local z = math.random(1,2)
							local LoopTp = game:GetService('RunService').Heartbeat:Connect(function()
								
								
									
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Root.CFrame * CFrame.new(0, 0, -2)
		
							end)
							_G.FinalLight = false
							local camera = workspace.CurrentCamera
							spawn(function()
								repeat camera.CFrame = CFrame.new(camera.CFrame.Position, Root.Position)  task.wait() until _G.FinalLight == true -- aqui faz a camera mirar na luz
							end)
							task.wait(0.1)
							TextStatus.Text = 'Items already taken: '.._G.ItemsPego..'\n'..'Pickuping: '..Root.Parent.Name..'\n'..'Status: pressing E'
							for i = 1,5 do

								keypress(0x35)
								
								task.wait()
								
								keyrelease(0x35)

								task.wait()
							end
							keypress(0x45) 
							wait(0.8)
							keyrelease(0x45) 
							if game.Players.LocalPlayer.Settings.IsHolding.Value == false then
								task.wait()
								keypress(0x45) 
								wait(0.8)
								keyrelease(0x45) 
							end
							TextStatus.Text = 'Items already taken: '.._G.ItemsPego..'\n'..'Pickuping: '..Root.Parent.Name..'\n'..'Status: Stopping pressing E'
							LoopTp:Disconnect()
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(150, 300, 150)
							TextStatus.Text = 'Items already taken: '.._G.ItemsPego..'\n'..'Pickuping: '..Root.Parent.Name..'\n'..'Status: Teleporting to CFrame.new(200, 300, 200)'
							wait()
							_G.FinalLight = true 
							wait()
							TextStatus.Text = 'Items already taken: '.._G.ItemsPego..'\n'..'Pickuping: '..Root.Parent.Name..'\n'..'Status: Dropping '..Root.Parent.Name
							if game.Players.LocalPlayer.Settings.IsHolding.Value == true then
								repeat
									keypress(0x45) 
									wait()
									keyrelease(0x45) 
									wait()
									workspace.CurrentCamera.CFrame = CFrame.new(workspace.CurrentCamera.CFrame.Position, Vector3.new(math.random(0,10000),math.random(0,10000),math.random(0,10000)))
								until game.Players.LocalPlayer.Settings.IsHolding.Value == false									
							end
						until (oldPOSROOT - Root.Position).Magnitude >=30 or vezes >= 3
						if vezes ~= 3 then
							_G.ItemsPego = _G.ItemsPego + 1
						end
					end
				end
			end
		end
    end
	_G.FinalDestroy = true
	DestroyAllGuis()
	noclip2(false)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldposDESTROY

end)


DestroySection:NewButton('Stop destroy', nil, function()
	_G.DestroyRunning = false
end)

_G.EspColor = Color3.new(0, 0, 255)
_G.EspEnabled = false
_G.EspThickness = 2

EspSection:NewButton('Active esp', nil, function()
	local FOLDER = game.Workspace:FindFirstChild('EspFolder')
	local pasta;
	if FOLDER then
		FOLDER:Destroy()
		pasta = Instance.new('Folder', workspace)
		pasta.Name = 'EspFolder'
	else
		pasta = Instance.new('Folder', workspace)
		pasta.Name = 'EspFolder'
	end
	local camera = workspace.CurrentCamera
	for i,v in pairs(game.Players:GetChildren()) do
		local Line = Drawing.new('Line')
		Line.Thickness = _G.EspThickness
		Line.Transparency = 1
		Line.Color = Color3.fromRGB(0, 0, 255)-- azul acho
		Line.Visible = false
		function UpdateTracers()
			game.RunService.RenderStepped:Connect(function()
				if v ~= game.Players.LocalPlayer then
					if v.Character ~= nil and
					v ~= nil and
					v.Character:FindFirstChild('HumanoidRootPart') ~= nil and
					v.Character:FindFirstChild('Humanoid') ~= nil and
					v.Character:FindFirstChild('Humanoid').Health > 0 then
						local Vector, OnScreen = camera:worldToViewportPoint(v.Character.HumanoidRootPart.Position)
		
						if OnScreen then
							Line.Visible = _G.EspEnabled
							Line.Thickness = _G.EspThickness
							Line.From = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y / 1)
							Line.To = Vector2.new(Vector.X, Vector.Y)
							Line.Color = _G.EspColor
						else
							Line.Visible = false
						end
					end
				else
					Line.Visible = false
				end
			end)
		end
		coroutine.wrap(UpdateTracers)()
	end
end)

EspSection:NewToggle('Esp Enabled', nil, function(bool)
	_G.EspEnabled = bool
end)

