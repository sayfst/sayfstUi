end)

local loopSafe
LocalPlayer:NewToggle('Safe local loop', nil, function(bool)
    if bool == true then
        loopSafe = game.RunService.Heartbeat:Connect(function()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, -20, 0)
        end)
    else
        loopSafe:Disconnect()
    end
end)

LocalPlayer:NewButton('Render map', nil, function()
    local map0 = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    local map1 = CFrame.new(1.0677889585494995, 5.500000476837158, 1.450244426727295)
    local map2 = CFrame.new(2545.563720703125, 5.5, 2.398519992828369)
    local map3 = CFrame.new(2546.072265625, 5.5, 2544.44287109375)
    local map4 = CFrame.new(3.25943660736084, 5.499999523162842, 2546.0224609375)
    local map5 = CFrame.new(1250.47900390625, 5.500000953674316, 1250.2767333984375)
    
    local TweenService = game:GetService('TweenService')
    
    function CreateTeleport(cframeA, tempo)
        TweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(tempo, Enum.EasingStyle.Linear), {CFrame = cframeA}):Play()
        wait(tempo)
    end
    
    CreateTeleport(map1, 0.5)
    wait(.5)
    CreateTeleport(map2, 6)
    CreateTeleport(map3, 6)
    CreateTeleport(map4, 6)
    CreateTeleport(map1, 6)
    wait(1)
    CreateTeleport(map5, 2)
    CreateTeleport(map2, 2)
    CreateTeleport(map4, 2)
    CreateTeleport(map5, 2)
    CreateTeleport(map4, 2)
    CreateTeleport(map0, 0.1)
end)

LocalPlayer:NewButton('FindHumanoidRootPart', 'he will render game', function()
    oldCFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    _G.Find = true
    repeat
        local Pos = math.random(0, 1250)
        local Pos2 = math.random(0, 1250)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Pos, 10, Pos2)
        task.wait()
    until PlayerSelect.Character:FindFirstChild('HumanoidRootPart') ~= nil or _G.Find == false
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldCFrame
    wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldCFrame
    wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldCFrame
    wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldCFrame
    wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldCFrame
    wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldCFrame
    wait()
    
end)
LocalPlayer:NewButton('Stop Find HumanoidRootPart', 'he will find humanoidRootpart', function()
    _G.Find = false
end)

