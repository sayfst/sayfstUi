	_G.EspThickness = value
end)
_G.AutoRefresh = false
GUI2:NewToggle('Auto refresh', nil, function(bool)
    _G.AutoRefresh = bool
end)
GUI2:NewButton('Refresh', nil, function()
    local OLDPOS = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    game.Players.LocalPlayer.Character.Humanoid.Health = 0
    game.Players.LocalPlayer.CharacterAdded:wait():WaitForChild('HumanoidRootPart')
    wait(.1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = OLDPOS 
end)
GUI2:NewButton('Como usa o auto refresh? (TUTORIAL)', nil, function()
    game.StarterGui:SetCore("SendNotification", {
        Title = 'Aperte "F9" para abrir o console';
        Text = 'e ver o tutorial';
    })
    print('\n-- AUTO REFRESH TUTORIAL, By $ samuu#6783\nAo ativar o toggle acima, voce esta ativando o auto refresh\nao morrer, ele salva sua posicao, e teleporta para ela\nao reviver, Assim voce nao perde casas, essas coisas...\nobrigado por usar Meu menu :)')
end)
function REFRESH()
    game.Players.LocalPlayer.Character:WaitForChild('Humanoid').Died:Connect(function()
        if _G.AutoRefresh then
            local OLDPOS = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            game.Players.LocalPlayer.CharacterAdded:wait():WaitForChild('HumanoidRootPart')
            wait(.1)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = OLDPOS 
            REFRESH()
        else
            REFRESH()
        end
    end)
end
REFRESH()

