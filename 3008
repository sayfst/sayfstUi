function Pickup(item)
    local args = {
        [1] = {
            ["Action"] = "Pickup",
            ["Model"] = workspace.GameObjects.Physical.Map.Ground.horizontalShelf_end.Items.Pallet
        }
    }
    game:GetService("Players").LocalPlayer.Character.server_PickupSystem.MainEvent:InvokeServer(unpack(args))

end


function ClearNoclips()
    for i,v in pairs(workspace.NOCLIPS:GetChildren()) do
        if v then
            v:Destroy()
        end
    end
end

local NOCLIPSFOLDER = workspace:FindFirstChild('NOCLIPS')

function CreateNoclipFolder()
    local NEWFOLDER = Instance.new('Folder')
    NEWFOLDER.Name = 'NOCLIPS'
    NEWFOLDER.Parent = workspace
end

if NOCLIPSFOLDER then
    NOCLIPSFOLDER:Destroy()
    task.wait()
    CreateNoclipFolder()
else
    CreateNoclipFolder()
end

------- CUSTOM FLY -------
pcall(function()
function noclip2(t)
    if KRNL_LOADED then
        if t == true then
            local NOCLIPPART = Instance.new('Part')
            NOCLIPPART.Name = 'NOCLIPPART'
            NOCLIPPART.Size = Vector3.new(5, 1, 5)
            NOCLIPPART.Position = Vector3.new(0, 0, 0)
            NOCLIPPART.Parent = workspace.NOCLIPS
            NOCLIPPART.Transparency = 1
            NOCLIPPART.CanCollide = false
            spawn(function()
                while task.wait() do
                    NOCLIPPART.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, -3.5, 0)
                end
            end)
        else
            ClearNoclips()
        end
    elseif syn then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/LegoHacker1337/legohacks/main/PhysicsServiceOnClient.lua"))()
        setfflag("HumanoidParallelRemoveNoPhysics", "False")
        setfflag("HumanoidParallelRemoveNoPhysicsNoSimulate2", "False")
        if t == true then
            game:GetService('RunService'):BindToRenderStep("crash", 0 , function()
                game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            end)
        elseif t == false then
            game:GetService('RunService'):UnbindFromRenderStep("crash")
        end
    end
end

function PickupLight(name, vezes, ROOTNAME)
	if _G.VezesLight == 'NotSelect' then
		local Aviso = CreateStatusGui()
		
		Aviso.Text = 'Notificacao:\nVoce nao selecionou\numa quantidade\nsalvo pelo samu'
		wait(6)
		DestroyAllGuis()

	else

		ROOTNAME = ROOTNAME or 'Root'
		wait(1)
		local oldpos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
		local camera = game:GetService('Workspace').CurrentCamera
		local vez = 0
		for i,v in pairs(game:GetService("Workspace").GameObjects.Physical.Map.Ground:GetDescendants()) do
			if v.Name:lower():match('^'..name:lower()) then
				if v:FindFirstChild(ROOTNAME) ~= nil then
					local Root = v:FindFirstChild(ROOTNAME)
					if (Root.Position - oldpos).Magnitude >= _G.DistanceItem  and vez ~= vezes then
						noclip2(true)
						local mouse = game.Players.LocalPlayer:GetMouse()
	
						local Part = Instance.new('Part', workspace)
						Part.Name = 'OldPosition'
						Part.Anchored = true
						Part.Size = Vector3.new(1, 1, 1)
						Part.Transparency = 0
						local hit = mouse.Hit
						Part.Position = Vector3.new(hit.X, hit.Y, hit.Z)
						vez = vez + 1
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Root.CFrame * CFrame.new(0, 0, 0)
						wait()
						local z = math.random(-2, 2)
						local LoopTp = game:GetService('RunService').RenderStepped:Connect(function()
							
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Root.CFrame * CFrame.new(0, 0, -2)
							
	
						end)
						_G.FinalLight = false
						spawn(function()
							repeat camera.CFrame = CFrame.new(camera.CFrame.Position, Root.Position)  task.wait() until _G.FinalLight == true -- aqui faz a camera mirar na luz
						end)
						wait()
						keypress(0x45) 
						wait(0.8)
						keyrelease(0x45) 
						LoopTp:Disconnect()
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(oldpos)
						wait()
						_G.FinalLight = true 
						camera.CFrame = CFrame.new(camera.CFrame.Position, Part.Position)
						Part:Destroy()
						wait(.5)
						if _G.VezesLight ~= 1 then
							keypress(0x45) 
							wait()
							keyrelease(0x45) 
							wait()
							if game.Players.LocalPlayer.Settings.IsHolding.Value == true then
								repeat
									keypress(0x45) 
									wait()
									keyrelease(0x45) 
									wait()
									workspace.CurrentCamera.CFrame = CFrame.new(workspace.CurrentCamera.CFrame.Position, Vector3.new(math.random(0,10000),math.random(0,10000),math.random(0,10000)))
								until game.Players.LocalPlayer.Settings.IsHolding.Value == false
							end
							
							noclip2(false)
						end
						noclip2(false)
					end
				else
					print('Error, Root is nil')
				end
			end
		end
	end
end

BaseItems:NewTextBox('Expecif', nil, function(string)
    string = string:split(',')

	PickupLight(string[1], _G.VezesLight, string[2])
end)

