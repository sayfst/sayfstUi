	end
end)
game.Players.LocalPlayer.Chatted:Connect(function(msg)
	msg = msg:split(' ')

	if msg[1] == '.velocidade' then
		_G.velocidade = tonumber(msg[2])
	end
end)
_G.velocidade = 3
local Benx;
LocalPlayer:NewToggle('Benx - plr', nil, function(bool)
	_G.Benx = bool
	if bool then
		bangAnim = Instance.new("Animation")
		bangAnim.AnimationId = "rbxassetid://148840371"
		local Track = game.Players.LocalPlayer.Character:WaitForChild('Humanoid'):LoadAnimation(bangAnim)
		Track:AdjustSpeed(_G.velocidade)
		local away = 0.5
		local reverse = false
		
		Track:Play()
		Benx = game:GetService('RunService').Heartbeat:Connect(function()
			if reverse == false then
				away = away + 0.1
			else
				away = away - 0.1
			end
			if away >=2 then
				reverse = true
			elseif away <= 0.5 then
				reverse = false
			end
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = PlayerSelect.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, away)
		
		end)
		repeat task.wait() until _G.Benx == false
		Benx:Disconnect()
		Track:Stop()
	end
end)

LocalPlayer:NewToggle('Give food - plr', nil, function(bool)
	getgenv().GiveFood = bool
	spawn(function()
		while getgenv().GiveFood == true do
			local lplr = game.Players.LocalPlayer
			local oldCFrame = lplr.Character.HumanoidRootPart.CFrame
			local oldPosition = lplr.Character.HumanoidRootPart.Position
			
			local ItemOnBackpack = 0
			game.RunService.RenderStepped:Connect(function()
				ItemOnBackpack = 0
				for i,v in pairs(lplr.Backpack:GetChildren()) do
					ItemOnBackpack = ItemOnBackpack + 1
				end
			end)
			
			for i,v in pairs(game:GetService("Workspace").GameObjects.Physical.Items:GetChildren()) do
				if v ~= nil then
					if v:FindFirstChild('Root') and 
					ItemOnBackpack <= 15 and 
					(v.Root.Position - oldPosition).Magnitude >= 25 then
						lplr.Character.HumanoidRootPart.CFrame = v.Root.CFrame
						task.wait()
						local args = {
							[1] = {
								["Action"] = "AlternateInteract",
								["Model"] = v,
							}
						}
						
						game:GetService("Players").LocalPlayer.Character.server_PickupSystem.MainEvent:InvokeServer(unpack(args))
						game:GetService("Players").LocalPlayer.Character.server_PickupSystem.MainEvent:InvokeServer(unpack(args))
						
						task.wait()
					end
				end
			end
			for i,v in pairs(game:GetService("Workspace").GameObjects.Physical.Map.Ground:GetChildren()) do
				if v ~= nil then
					if v:FindFirstChild('Root') ~= nil and
					ItemOnBackpack <= 15 and
					(v.Root.Position - oldPosition).Magnitude >= 25 then
						lplr.Character.HumanoidRootPart.CFrame = v.Root.CFrame
						task.wait()
						local args = {
							[1] = {
								["Action"] = "AlternateInteract",
								["Model"] = v,
							}
						}
						
						game:GetService("Players").LocalPlayer.Character.server_PickupSystem.MainEvent:InvokeServer(unpack(args))
						game:GetService("Players").LocalPlayer.Character.server_PickupSystem.MainEvent:InvokeServer(unpack(args))
						
						task.wait()
					end
				end
			end
		
			lplr.Character.HumanoidRootPart.CFrame = PlayerSelect.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0)
			wait(1)
			local camera = workspace.CurrentCamera
			camera.CFrame = CFrame.new(camera.CFrame.Position, PlayerSelect.Character.HumanoidRootPart.Position + Vector3.new(0, -3.5, 0))
			task.wait(.1)
			game.Players.LocalPlayer.Character.Humanoid.Health = 0
			game.Players.LocalPlayer.CharacterAdded:wait():WaitForChild('HumanoidRootPart')
			lplr.Character.HumanoidRootPart.CFrame = oldCFrame

		end
	end)
end)

