                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 1, -16)
                            wait()
                        end
                    end)
                end
            end)
            
            game.Players.LocalPlayer.Backpack.ChildAdded:Connect(function(child)
                if child.Name == Tool.Name then
                    getgenv().Teleport = false
                end
            end)
        end)
        
