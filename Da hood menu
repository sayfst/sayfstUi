                player = game.Players.LocalPlayer
                Character = player.Character or player.CharacterAdded:wait()

                Humanoid = Character:WaitForChild("Humanoid")

                Animation = Humanoid:LoadAnimation(game:GetService("ReplicatedStorage").ClientAnimations.Crouching)
                Animation.Looped = false
                if args[1] == prefix .. 'benx' then
                    if args[2] == '.' then
                        pcall(function()
        
                            local Target = getgenv().OwnerName
                            getgenv().Benx = true
                            local Player = game.Players:FindFirstChild(Target)
                            
                            local away = .5
                            
                            local reversing = true
                            
                            function benx()
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -2.7 + away)
                                if reversing == true then
                                    away = away - 0.1
                                else
                                    away = away + 0.1
                                end
                                if away >= 2 then
                                    reversing = true
                                elseif away < 0.5 then
                                    reversing = false
                                end
                            end
                        
                            local Start = function()
                                _G.Loop = game:GetService("RunService").Heartbeat:Connect(benx);
                                Animation:Play()
                            end
                            
                            Start()
                            repeat wait() until getgenv().Benx == false
                            _G.Loop:Disconnect()
                            Animation:Stop()
                            Animation:Stop()
                            Animation:Stop()
                            
                            wait(.2)
                        end)
                    elseif args[2] == game.Players.LocalPlayer.Name or args[2] == game.Players.LocalPlayer.DisplayName then
                        local Target = getgenv().OwnerName
                        getgenv().Benx = true
                        local Player = game.Players:FindFirstChild(Target)
                        
                        local away = .5
                        
                        player = game.Players.LocalPlayer
                                    Character = player.Character or player.CharacterAdded:wait()
                    
                                    Humanoid = Character:WaitForChild("Humanoid")
                    
                                    Animation = Humanoid:LoadAnimation(game:GetService("ReplicatedStorage").ClientAnimations.Crouching)
                                    Animation.Looped = true
                                    Animation:Play()
                      
                        local reversing = true
                        
                        function benx()
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -3 + away)
                            if reversing == true then
                                away = away - 0.1
                            else
                                away = away + 0.1
                            end
                            if away >= 2 then
                                reversing = true
                            elseif away < 0.5 then
                                reversing = false
                            end
                        end
                    
                        local Start = function()
                            _G.Loop = game:GetService("RunService").Heartbeat:Connect(benx);
                        
                            
                        end
                        
