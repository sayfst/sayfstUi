Main:CreateButton('Back', function()
    _G.Now = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().BackPosition
    wait(.8)
    Pickup(false)
    wait(.2)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = _G.Now
end)

Main:CreateButton('Target', function(button)
    if button.Text == 'Target' then
        button.Text = 'UnTarget'
        local player = game.Players.LocalPlayer
        pcall(function()
            player.Character.LeftHand.LeftWrist:Destroy();
            player.Character.RightHand.RightWrist:Destroy();
        end);
        function LoopFunction()
            player.Character.LeftHand.CFrame = CFrame.new(getgenv().Player.Character.UpperTorso.Position)
            player.Character.RightHand.CFrame = CFrame.new(getgenv().Player.Character.UpperTorso.Position)
        end

        
        local Start = function()
            getgenv().LoopTarget = game:GetService('RunService').Heartbeat:Connect(LoopFunction)
        end
        local Stop = function()
            getgenv().LoopTarget:Disconnect()
            player.Character.LeftHand.CFrame  = player.Character.LeftLowerArm.CFrame
            player.Character.RightHand.CFrame = player.Character.RightLowerArm.CFrame
        end

        Start()
        repeat wait() until button.Text == 'Target' or getgenv().Player.Character.Humanoid.Health <=5
        Stop()

    elseif button.Text == 'UnTarget' then
        button.Text = 'Target'
    end
end)

Main:CreateButton('Arrest 2', function()
    local OldCFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    local HumanoidRootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
    Pickup(false)
    wait(.5)
    local Cuff = game.Players.LocalPlayer.Character:FindFirstChild('Cuff') or game.Players.LocalPlayer.Backpack:FindFirstChild('Cuff')
    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
        if v:IsA("Tool") then
            v.Parent = game.Players.LocalPlayer.Backpack
        end
    end
    Cuff.Parent = game.Players.LocalPlayer.Character

    spawn(function()
        for i = 1,50 do
            Cuff:Activate()
            wait()
        end
    end)
    for i = 1,50 do
        HumanoidRootPart.CFrame = CFrame.new(getgenv().Player.Character.UpperTorso.Position + Vector3.new(0, 1.5, 0))
        wait()
    end
    HumanoidRootPart.CFrame = OldCFrame
end)

