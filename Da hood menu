Main:CreateButton('Setup', function(button)
    if button.Text == 'Setup' then
        button.Text = 'UnSetup'
        local SetupAway = 0
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
        wait()
        getgenv().OldPosSetup = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        spawn(function()
            repeat SetupAway = SetupAway + .1 wait() until SetupAway == 3 or SetupAway >=3
        end)
        local Start = function()
            getgenv().Setup = game:GetService("RunService").Heartbeat:Connect(function()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().OldPosSetup + Vector3.new(0, SetupAway, 0)
            end)
        end
        local Stop = function()
            getgenv().Setup:Disconnect()
        end

        Start()
        repeat wait() until button.Text == 'Setup'
        Stop()

    elseif button.Text == 'UnSetup' then
        button.Text = 'Setup'
    end
end)
Main:CreateButton('Hide', function(button)
    if button.Text == 'Hide' then
        button.Text = 'UnHide'
        getgenv().OldPositionHide = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        local SetupAway = 0
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
        wait()
        getgenv().OldPosSetup = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        spawn(function()
            repeat SetupAway = SetupAway - 0.1 wait() until SetupAway == -7 or SetupAway <=-7
        end)
        local Start = function()
            getgenv().Setup = game:GetService("RunService").Heartbeat:Connect(function()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().OldPosSetup + Vector3.new(0, SetupAway, 0)
            end)
        end
        local Stop = function()
            getgenv().Setup:Disconnect()
        end

        Start()
        repeat wait() until button.Text == 'Hide'
        Stop()

    elseif button.Text == 'UnHide' then
        button.Text = 'Hide'
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().OldPositionHide
    end
end)


Main:CreateButton('Aim lock', function()
    local L_1_ = "t"
    local L_2_ = game.Players.LocalPlayer:GetMouse()
    L_2_.KeyDown:Connect(
        function(L_22_arg0)
        if L_22_arg0 == L_1_ then
            if DaHoodSettings.SilentAim == true then
                DaHoodSettings.SilentAim = false
            else
                DaHoodSettings.SilentAim = true
            end
        end
    end
    )
    game:GetService("RunService").RenderStepped:Connect(
        function()
        for L_23_forvar0, L_24_forvar1 in pairs(game.CoreGui:GetChildren()) do
            if L_24_forvar1.Name == "PostmansAutoRob" then
                L_24_forvar1:Destroy()
            end
        end
        for L_25_forvar0, L_26_forvar1 in pairs(game.CoreGui:GetChildren()) do
            if L_26_forvar1.Name == "WarningGUI" then
                L_26_forvar1:Destroy()
            end
        end
    end
    )
    local L_3_ = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local L_4_ = L_3_.CreateLib("r4ware Softworks", "DarkTheme")
    local L_5_ = L_4_:NewTab("Aimlock")
    local L_6_ = L_5_:NewSection("r4ware Softworks")
    L_6_:NewButton(
        "Aimlock",
        "Locks on the user",
        function()
        getgenv().AimPart = "HumanoidRootPart"
        getgenv().AimlockKey = "q"
        getgenv().AimRadius = 30
