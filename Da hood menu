                local Pray = Humanoid:LoadAnimation(PrayAni)
                

                if args[1] == prefix .. 'pray' then
                    if args[2] == '.' then
                        Pray:Play()
                    elseif args[2] == game.Players.LocalPlayer.Name then
                        Pray:Play()
                    end
                end

                if args[1] == prefix .. 'vibe' then
                    if args[2] == '.' then
                        Dance1:Play()
                    elseif args[2] == game.Players.LocalPlayer.Name then
                        Dance1:Play()
                    end
                end
                if args[1] == prefix .. 'stop' then
                    if args[2] == '.' then
                        Dance1:Stop()
                    elseif args[2] == game.Players.LocalPlayer.Name then
                        Dance1:Stop()
                    end
                end
                if args[1] == prefix .. 'cuffban' then
                    game.Players.LocalPlayer:Kick('Stop .')
                end

                if say[1] == prefix .. 'warn' then
                    local message = Instance.new("Message", game:GetService("CoreGui"))
                    message.Text = say[2]..': '..say[3]
                    wait(3)
                    message:Destroy()
                end
                if args[1] == prefix .. 'santana' then
                    if args[2] == '.' then
                        local Sound = Instance.new("Sound", workspace)
                        Sound.Name = '65675df64f4d5h45f43df243f2d34h3f2d'
                        Sound.SoundId = 'rbxassetid://149063527'
                        Sound:Play()
                        Sound.Ended:wait()
                        Sound:Destroy()
                    elseif args[2] == game.Players.LocalPlayer.Name or args[2] == game.Players.LocalPlayer.DisplayName then
                        local Sound = Instance.new("Sound", workspace)
                        Sound.Name = '65675df64f4d5h45f43df243f2d34h3f2d'
