Main:CreateButton('Fling', function(button)
    if button.Text == 'Fling' then
        getgenv().FlingOldPosition = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        button.Text = 'UnFling'
        
        function tteleport()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().Player.Character.HumanoidRootPart.CFrame
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(50000000, 50000000, 50000000)
        end
        local Start = function()
            getgenv().Fling = game:GetService("RunService").Heartbeat:Connect(tteleport)
        end
        Start()
    elseif button.Text == 'UnFling' then
        button.Text = 'Fling'
        getgenv().Fling:Disconnect()
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
        game.Players.LocalPlayer.Character.PrimaryPart.Velocity = Vector3.new(0, 0, 0)
             
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().FlingOldPosition
    end
end)

Main:CreateButton('Save player', function()
    Now = Character.HumanoidRootPart.Position
    local Position1 = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
    local Position2 = getgenv().Player.Character.UpperTorso.Position
    repeat
        for i = 1,10 do
            Teleport(CFrame.new(getgenv().Player.Character.UpperTorso.Position + Vector3.new(0, 1.5, 0)))
            local Position2 = getgenv().Player.Character.UpperTorso.Position
            wait()
        end
        local Position2 = getgenv().Player.Character.UpperTorso.Position
        Pickup(true)
        local Position2 = getgenv().Player.Character.UpperTorso.Position
        wait(.1)
        local Position2 = getgenv().Player.Character.UpperTorso.Position
        Character.HumanoidRootPart.CFrame = CFrame.new(Now)
        wait(.2)
        Pickup(false)
        local Position2 = getgenv().Player.Character.UpperTorso.Position
        wait(.1)
        local Position2 = getgenv().Player.Character.UpperTorso.Position
    until (Position1 - Position2).Magnitude <=15 or getgenv().RunningBring == false
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Now
end)

