                                game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid").PlatformStand = false
                            end
                        end
                            )
                    end
                    IYMouse.KeyDown:connect(
                            function(L_165_arg0)
                        if L_165_arg0:lower() == "w" then
                            if L_157_arg0 then
                                L_159_.F = vehicleflyspeed
                            else
