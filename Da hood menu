function Pickup(state)
    local args = {
        [1] = "Grabbing",
        [2] = state,
    }
    game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
end
function Stomp()
    game:GetService("ReplicatedStorage").MainEvent:FireServer('Stomp')
end


Main:CreateButton('Copy shirt', function()
    local shirt = game.Players.LocalPlayer.Character:FindFirstChild('Shirt')
    local shirt2 = getgenv().Player.Character:FindFirstChild('Shirt')
    if shirt2 and shirt then
        shirt.ShirtTemplate = shirt2.ShirtTemplate
    end
end)
Main:CreateButton('Copy pants', function()
    local pants = game.Players.LocalPlayer.Character:FindFirstChild('Pants')
    local pants2 = getgenv().Player.Character:FindFirstChild('Pants')
    if pants and pants2 then
        pants.PantsTemplate = pants2.PantsTemplate
    end
end)

Main:CreateButton('Copy face', function()
    local face = game.Players.LocalPlayer.Character.Head:FindFirstChild('face')
    local face2 = getgenv().Player.Character.Head:FindFirstChild('face')
    if face and face2 then
        face.Texture = face2.Texture
    end
end)
function setFace(id)
    local face = game.Players.LocalPlayer.Character.Head:FindFirstChild('face')
    if face then
        face.Texture = 'rbxassetid://'..id
    end
end
