                    workspace.CurrentCamera.CameraSubject = getgenv().Player.Character
                    game.ReplicatedStorage.MainEvent:FireServer('Block', true)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().Player.Character.HumanoidRootPart.CFrame + Vector3.new(0, -5, 0)
                end)
        
            elseif b.Text == 'UnHide drop' then
                b.Text = 'Hide drop - 100'
                getgenv().HideDrop:Disconnect()
                wait()
                game.ReplicatedStorage.MainEvent:FireServer('Block', false)
                workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().OldPos
            end
        end)
        
        
        local CashAura = false
        Main:CreateButton('CashAura', function(button)
           if button.Text == 'CashAura' then
               button.Text = 'UnCashAura'
               else
                   button.Text = 'CashAura'
           end
           CashAura = not CashAura
           _G.CashAura = CashAura
           spawn(function()
               while _G.CashAura == true do
                   for i,v in pairs(game:GetService("Workspace").Ignored.Drop:GetChildren()) do 
                       if v:IsA("Part") then
                           if (v.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <=30 then
                               fireclickdetector(v.ClickDetector)
                           end
                       end
                   end
                   wait()
               end
           end)
        end)
        
        Main:CreateButton('Auto drop', function(button)
            if button.Text == 'Auto drop' then
                button.Text = 'UnAuto drop'
                getgenv().AutoDrop = true
                spawn(function()
                    while getgenv().AutoDrop == true do
                        game.ReplicatedStorage.MainEvent:FireServer('DropMoney', '10000')
                        wait()
                    end
        
                end)
            elseif button.Text == 'UnAuto drop' then
                button.Text = 'Auto drop'
                getgenv().AutoDrop = false
            end
        end)
        
        Main:CreateButton('Auto MoneyGun', function(button)
            if button.Text == 'Auto MoneyGun' then
                button.Text = 'UnAuto MoneyGun'
                local MoneyGun = game.Players.LocalPlayer.Backpack:FindFirstChild('[Money Gun]') or game.Players.LocalPlayer.Character:FindFirstChild('[Money Gun]')
                getgenv().AutoMoneyGun = function()
                    spawn(function()
                        repeat
                            if MoneyGun then
                                MoneyGun.Parent = game.Players.LocalPlayer.Character
                                MoneyGun:Activate()
                            end
                            wait()
                        until button.Text == 'Auto MoneyGun'
                    end)
                end
                if MoneyGun then
                    getgenv().AutoMoneyGun()
                else
                    local Now = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                    repeat
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Ignored.Shop["[Money Gun] - $777"].Head.CFrame
                        fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Money Gun] - $777"].ClickDetector)
                        wait()
                    until game.Players.LocalPlayer.Backpack:FindFirstChild('[Money Gun]') ~= nil
                    game.Players.LocalPlayer.Backpack:FindFirstChild('[Money Gun]').Parent = game.Players.LocalPlayer.Character
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Now
                    wait()
                    local Now = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                    repeat
