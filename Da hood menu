    if WarnFarm == false then
        button.Text = 'Really?'
        delay(1, function()
            button.Text = 'Auto farm'
        end)
        WarnFarm = true
    elseif WarnFarm == true then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/lerkermer/lua-projects/master/SwagAutofarm"))()
    end
end)

