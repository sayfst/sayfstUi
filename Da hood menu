        LP = game.Players.LocalPlayer
        getgenv().RightHandOldSize = LP.Character.RightHand.Size
        getgenv().LeftHandOldSize = LP.Character.LeftHand.Size
        
        function Reach(state)
            getgenv().ReachState = state
            if state == true then
                LP = game.Players.LocalPlayer
                for i,v in ipairs(LP.Character:GetDescendants()) do
                if v:IsA("MeshPart") then v.Massless = true
                v.CanCollide = false
                v.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0)
                
                end
                end
                
                for i,v in ipairs(game.workspace:GetDescendants()) do
                if v:IsA("Seat") then 
                v.Disabled = true
                end
                end
                x = 35
                y = 35
                z = 35
                
                
                penis = Vector3.new(x,y,z)
                
                LP.Character.RightHand.Size = penis
                
                LP.Character.RightHand.Transparency = 1
                
                LP.Character.LeftHand.Size = penis
                LP.Character.BodyEffects.SpecialParts.LeftHand.Size = penis
                
                LP.Character.LeftHand.Transparency = 1
                
            elseif state == false then
                LP = game.Players.LocalPlayer
                LP.Character.LeftHand.Transparency = 0
                LP.Character.RightHand.Transparency = 0
                LP.Character.LeftHand.Size = getgenv().LeftHandOldSize
                LP.Character.RightHand.Size = getgenv().RightHandOldSize
            end
        end
        
        
        
        function KillPlayer(state, target)
            if getgenv().Player.Character then
                local Remote = game:GetService("ReplicatedStorage"):FindFirstChild("MainEvent") or game:GetService("Lighting"):FindFirstChild('MainEvent')
                Reach(true)
                Remote.Parent = game.Lighting
                local HumanoidRootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
                local OldCFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                local OldPosition = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                local combat = game.Players.LocalPlayer.Character:FindFirstChild('Combat') or game.Players.LocalPlayer.Backpack:FindFirstChild('Combat')
                
                
                function LoopFunction()
                    
                    combat.Parent = game.Players.LocalPlayer.Character
                    combat:Activate()
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().Player.Character.HumanoidRootPart.CFrame + Vector3.new(math.random(0, 2), -8, math.random(0, 2))
                end
            
                local Start = function()
                    getgenv().KillLoop = game:GetService('RunService').Heartbeat:Connect(LoopFunction)
                end
                local Stop = function()
                    getgenv().KillLoop:Disconnect()
                end
            
                Start()
                repeat wait() until getgenv().Player.Character.Humanoid.Health <=5 or HumanoidRootPart.Parent.Humanoid.Health <=5
            
                if HumanoidRootPart.Parent.Humanoid.Health <=5 then
                    game.Players.LocalPlayer.Character["HumanoidRootPart"]:Destroy() game.Players.LocalPlayer.Character["RightHand"]:Destroy() game.Players.LocalPlayer.Character["LeftLowerArm"]:Destroy() game.Players.LocalPlayer.Character["RightLowerArm"]:Destroy() game.Players.LocalPlayer.Character["LeftUpperArm"]:Destroy() game.Players.LocalPlayer.Character["RightUpperArm"]:Destroy() game.Players.LocalPlayer.Character["LowerTorso"]:Destroy() game.Players.LocalPlayer.Character["LeftLowerLeg"]:Destroy() game.Players.LocalPlayer.Character["LeftUpperLeg"]:Destroy() game.Players.LocalPlayer.Character["LeftFoot"]:Destroy() game.Players.LocalPlayer.Character["RightFoot"]:Destroy() game.Players.LocalPlayer.Character["RightLowerLeg"]:Destroy() game.Players.LocalPlayer.Character["RightUpperLeg"]:Destroy() game.Players.LocalPlayer.Character["UpperTorso"]:Destroy() game.Players.LocalPlayer.Character["LeftHand"]:Destroy() game.Players.LocalPlayer.Character["Head"]:Destroy()  
                    Remote.Parent = game.ReplicatedStorage
                    Stop()
                end
                if getgenv().Player.Character.Humanoid.Health <=5 then
                    Stop()
                    Reach(false)
                    getgenv().BackPosition = CFrame.new(getgenv().Player.Character.UpperTorso.Position + Vector3.new(0, 5, 0))
                    Remote.Parent = game.ReplicatedStorage
                    
                    HumanoidRootPart.CFrame = CFrame.new(getgenv().Player.Character.UpperTorso.Position + Vector3.new(0, 5, 0))
                    wait()
                    if state == 'bring' then
                        local Pos1 = OldPosition
                        wait(.4)
                        repeat
                            for i = 1,10 do
                                HumanoidRootPart.CFrame = CFrame.new(getgenv().Player.Character.UpperTorso.Position + Vector3.new(0, 1.5, 0))
                                wait()
                            end
                            Pickup(true)
                            for i = 1,4 do
                                HumanoidRootPart.CFrame = CFrame.new(getgenv().Player.Character.UpperTorso.Position + Vector3.new(0, 1.5, 0))
                                wait()
                            end
                            HumanoidRootPart.CFrame = OldCFrame
                            wait(.3)
                            Pickup(false)
                            wait(.1)
                        until (Pos1 - getgenv().Player.Character.UpperTorso.Position).Magnitude <=15 or getgenv().RunningBring == false
                        HumanoidRootPart.CFrame = OldCFrame
                    end
                    if state == 'arrest' then
                        local Cuff = game.Players.LocalPlayer.Character:FindFirstChild('Cuff') or game.Players.LocalPlayer.Backpack:FindFirstChild('Cuff')
                        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                            if v:IsA("Tool") then
                                v.Parent = game.Players.LocalPlayer.Backpack
                            end
                        end
                        Cuff.Parent = game.Players.LocalPlayer.Character
            
                        spawn(function()
                            for i = 1,50 do
                                Cuff:Activate()
                                wait()
                            end
                        end)
                        for i = 1,50 do
                            HumanoidRootPart.CFrame = CFrame.new(getgenv().Player.Character.UpperTorso.Position + Vector3.new(0, 1.5, 0))
                            wait()
                        end
                        HumanoidRootPart.CFrame = OldCFrame
                    end
                    if state == 'kill' then
                        HumanoidRootPart.CFrame = OldCFrame
                    end
                    if state == 'stomp' then
                        for i = 1,50 do
                            HumanoidRootPart.CFrame = CFrame.new(getgenv().Player.Character.UpperTorso.Position + Vector3.new(0, 1.5, 0))
                            Stomp()
                            wait()
                        end
                        HumanoidRootPart.CFrame = OldCFrame
                    end
                end
            end
        end
        
        
        
        Main:CreateButton('Auto buy menu', function()
            -- Gui to Lua
            -- Version: 3.2
            
            -- Instances:
            
            local ScreenGui = Instance.new("ScreenGui")
            local Frame = Instance.new("Frame")
            local TextLabel = Instance.new("TextLabel")
            local ScrollingFrame = Instance.new("ScrollingFrame")
            local TextButton = Instance.new("TextButton")
            local UIGridLayout = Instance.new("UIGridLayout")
            
            --Properties:
            
            ScreenGui.Parent = game.CoreGui
            ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
            
            Frame.Parent = ScreenGui
            Frame.AnchorPoint = Vector2.new(0.5, 0.5)
            Frame.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
            Frame.BorderSizePixel = 0
            Frame.Position = UDim2.new(0.5, 0, 0.499411076, 0)
            Frame.Size = UDim2.new(0, 997, 0, 635)
            
            TextLabel.Parent = Frame
            TextLabel.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
            TextLabel.BorderSizePixel = 0
            TextLabel.Size = UDim2.new(0, 997, 0, 64)
            TextLabel.Font = Enum.Font.SourceSans
            TextLabel.Text = "DA HOOD AUTO BUY v1"
            TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextLabel.TextScaled = true
            TextLabel.TextSize = 14.000
            TextLabel.TextWrapped = true
            
            ScrollingFrame.Parent = Frame
            ScrollingFrame.Active = true
            ScrollingFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ScrollingFrame.BackgroundTransparency = 1.000
            ScrollingFrame.Position = UDim2.new(0, 0, 0.100787401, 0)
            ScrollingFrame.Size = UDim2.new(0, 997, 0, 571)
            
            TextButton.Parent = ScrollingFrame
            TextButton.BackgroundColor3 = Color3.fromRGB(54, 54, 54)
            TextButton.BorderSizePixel = 0
            TextButton.Position = UDim2.new(0.23370111, 0, 0.167716533, 0)
            TextButton.Size = UDim2.new(0, 200, 0, 50)
            TextButton.Visible = false
            TextButton.Font = Enum.Font.SourceSans
            TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextButton.TextScaled = true
            TextButton.TextSize = 14.000
            TextButton.TextWrapped = true
            
            UIGridLayout.Parent = ScrollingFrame
            UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
            UIGridLayout.CellSize = UDim2.new(0, 192, 0, 35)
            
            -- Scripts:
            
            local function GOCH_fake_script() -- ScrollingFrame.LocalScript 
                local script = Instance.new('LocalScript', ScrollingFrame)
            
                game.StarterGui:SetCore("SendNotification", {
                    Title = 'Da hood auto buy';
                    Text = 'press "RightControl" to Toggled visible menu';
                })
                
                local UserInputService = game:GetService("UserInputService")
                UserInputService.InputBegan:Connect(function(input)
                    if input.KeyCode == Enum.KeyCode.RightControl then
                        script.Parent.Parent.Visible = not script.Parent.Parent.Visible
                    end
                end)
                
                for i,v in pairs(game:GetService("Workspace").Ignored.Shop:GetChildren()) do
                    if v:IsA("Model") then
                        local Button = script.Parent.TextButton:Clone()
                        Button.Parent = script.Parent
                        Button.Visible = true
                        Button.Text = v.Name
                        Button.MouseButton1Click:Connect(function()
                            local Now = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Head.CFrame
                            wait(0.3)
                            fireclickdetector(v.ClickDetector)
                            wait(.1)
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Now
                        end)
                    end
                end
            end
            coroutine.wrap(GOCH_fake_script)()
            
            end)
        
        
        
