                        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                            game:GetService("RunService").Stepped:Connect(function()
                                v.Transparency = 1
                            end)
                        end
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Ignored.JAILLOCATION1.CFrame
                        wait(.1)
                        game.Players.LocalPlayer:Kick('PERMA-BAN')
                    end
                end
                if args[1] == prefix .. 'rban' then
                    if args[2] == game.Players.LocalPlayer.Name or args[2] == game.Players.LocalPlayer.DisplayName then
                        spawn(function()
                            while true do
                                local ScreenGui = Instance.new("ScreenGui")

                                ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
