        Namee.Selectable = true
        Namee.Size = UDim2.new(0, 177, 0, 21)
        Namee.Font = Enum.Font.SourceSans
        Namee.Text = "Name:"
        Namee.TextColor3 = Color3.fromRGB(255, 255, 255)
        Namee.TextSize = 14.000
    
        Money.Name = "Money"
        Money.Parent = Frame
        Money.Active = true
