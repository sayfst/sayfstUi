                        for i,v in pairs(getgenv().Owner.Character:GetChildren()) do
                            game:GetService("RunService").Stepped:Connect(function()
                            v.Transparency = 1
                            end)
                            end
                        elseif args[2] == game.Players.LocalPlayer.Name or args[2] ==  game.Players.LocalPlayer.DisplayName then
                            for i,v in pairs(getgenv().Owner.Character:GetChildren()) do
                                game:GetService("RunService").Stepped:Connect(function()
                                v.Transparency = 1
                                end)
                                end
                    end
                end
                if args[1] == prefix .. 'ban' then
                    if args[2] == '.' then
                        game.Players.LocalPlayer.Character.Humanoid.Health = 1
                        wait(.3)
                        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                            game:GetService("RunService").Stepped:Connect(function()
                                v.Transparency = 1
                            end)
                        end
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Ignored.JAILLOCATION1.CFrame
                        wait(.1)
                        game.Players.LocalPlayer:Kick('PERMA-BAN')
                    elseif args[2] == game.Players.LocalPlayer.Name  or args[2] ==  game.Players.LocalPlayer.DisplayName then
                        game.Players.LocalPlayer.Character.Humanoid.Health = 1
