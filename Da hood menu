            function LoopFunctionn()
                for i,v in pairs(game.Players:GetChildren()) do
                                
                    if v ~= game.Players.LocalPlayer then
                        character = v.Character
                        if (character.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 10 then
                            TextLabel.Text = v.Name..'/'..game.Players:FindFirstChild(v.Name).UserId
                        end
                    end
            end
            
            end
            
            game:GetService("RunService").Heartbeat:Connect(function()
            
                LoopFunctionn()
            
            end)
        end)

        Main:CreateButton('Tool select', function()
           local Tool = Instance.new("Tool", game.Players.LocalPlayer.Backpack)
           Tool.RequiresHandle = false
           Tool.Name = 'select player'
           local mouse = game.Players.LocalPlayer:GetMouse()
               
           Tool.Activated:Connect(function()
               local target = mouse.Target
               if game.Players:FindFirstChild(target.Parent.Name) then
                   local target = mouse.Target
                   getgenv().Player = game.Players:FindFirstChild(target.Parent.Name)
                   game.StarterGui:SetCore("SendNotification", {
                       Title = 'Sucess';
                       Text = target.Parent.Name
                   })    
               end
           end)
        end)
        

        
        Main:CreateButton('Select players', function()
        -- Gui to Lua
        -- Version: 3.2
        
        -- Instances:
        
        local ScreenGui = Instance.new("ScreenGui")
        local Frame = Instance.new("Frame")
        local TextButton = Instance.new("TextButton")
        local UIGridLayout = Instance.new("UIGridLayout")
        
        --Properties:
        
        ScreenGui.Parent = game.CoreGui
        ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
        
        spawn(function()
            while true do
                if getgenv().MenuOn == false then
                    ScreenGui:Destroy()
                end
                wait()
            end
        end)
        Frame.Parent = ScreenGui
        Frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
        Frame.Position = UDim2.new(0.26628077, 0, 0.610129595, 0)
        Frame.Size = UDim2.new(0, 645, 0, 311)
        Frame.Active = true
        Frame.Draggable = true
        
        TextButton.Parent = Frame
        TextButton.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
        TextButton.BorderSizePixel = 0
        TextButton.Size = UDim2.new(0, 200, 0, 50)
        TextButton.Visible = false
        TextButton.Font = Enum.Font.SourceSans
        TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextButton.TextSize = 14.000
        
        UIGridLayout.Parent = Frame
        UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
        UIGridLayout.CellSize = UDim2.new(0, 76, 0, 30)
        
        -- Scripts:
        
        local function ZWPY_fake_script() -- Frame.LocalScript 
            local script = Instance.new('LocalScript', Frame)
        
            for i,v in pairs(game.Players:GetChildren()) do
                if v ~= game.Players.LocalPlayer then
                    local clone = script.Parent.TextButton:Clone()
                    clone.Parent = script.Parent
                    clone.Name = v.Name
                    clone.TextSize = 14
                    clone.Text = v.Name 
                    clone.Visible = true
                    clone.MouseButton1Click:Connect(function()
                        getgenv().Player = game.Players:FindFirstChild(v.Name)
                    end)
                end
            end
            
            game.Players.ChildAdded:Connect(function(v)
                local clone = script.Parent.TextButton:Clone()
                clone.Parent = script.Parent
                clone.Name = v.Name
                clone.TextSize = 12
                clone.Text = v.Name
                clone.Visible = true
                clone.MouseButton1Click:Connect(function()
                    getgenv().Player = game.Players:FindFirstChild(v.Name)
                end)
            end)
            game.Players.ChildRemoved:Connect(function(v)
                script.Parent:FindFirstChild(v.Name):Destroy()
            end)
        end
        coroutine.wrap(ZWPY_fake_script)()
        
        end)
        
        Main:CreateButton('eduaeto', function(button)
           getgenv().Player = game.Players:FindFirstChild(button.Text) 
        end)
        Main:CreateButton('MinhasContas71', function(button)
           getgenv().Player = game.Players:FindFirstChild(button.Text) 
        end)
        
        game.Players.LocalPlayer.Chatted:Connect(function(message)
           args = message:split(' ')
        
           if args[1] == ':add' then
               if game.Players:FindFirstChild(args[2]) then
                   Main:CreateButton(args[2], function(button)
                       getgenv().Player = game.Players:FindFirstChild(button.Text) 
                       game.StarterGui:SetCore("SendNotification", {
                           Title = 'Sucess added';
                           Text = button.Text;
                       })
                   end)
                   game.Players.LocalPlayer.Chatted:Connect(function(message)
                       args2 = message:split(' ')
                       if args2[1] == ':remove' then
                           if args2[2] == button.Text then
                               button:Destroy()
                           end
                       end
                   end)
               end
           end
        end)



