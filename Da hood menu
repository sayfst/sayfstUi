local Bexing = false
Main:CreateButton('Benx', function(button)
    if button.Text == 'Benx' then
        button.Text = 'UnBenx'
        pcall(function()
        
            local Target = getgenv().Player.Name
            getgenv().Benx = true
            local Player = game.Players:FindFirstChild(Target)
            
            local away = .5
            
            player = game.Players.LocalPlayer
                        Character = player.Character or player.CharacterAdded:wait()
        
                        Humanoid = Character:WaitForChild("Humanoid")
        
                        Animation = Humanoid:LoadAnimation(game:GetService("ReplicatedStorage").ClientAnimations.Crouching)
                        Animation.Looped = true
                        Animation:Play()
          
            local reversing = true
            
            function benx()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -3 + away)
                if reversing == true then
                    away = away - 0.1
                else
                    away = away + 0.1
                end
                if away >= 2 then
                    reversing = true
                elseif away < 0.5 then
                    reversing = false
                end
            end
        
            local Start = function()
                _G.Loop = game:GetService("RunService").Heartbeat:Connect(benx);
            
                
            end
            
            Start()
            repeat wait() until getgenv().Benx == false
            _G.Loop:Disconnect()
            Animation:Stop()
        end)
    elseif button.Text == 'UnBenx' then
        button.Text = 'Benx'
        getgenv().Benx = false
    end
end)

Main:CreateButton('Hide drop - 10k', function(b)
    if b.Text == 'Hide drop - 10k' then
        getgenv().OldPos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        b.Text = 'UnHide drop'
        getgenv().HideDrop = game:GetService("RunService").Heartbeat:Connect(function()
            workspace.CurrentCamera.CameraSubject = getgenv().Player.Character
            game.ReplicatedStorage.MainEvent:FireServer('Block', true)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().Player.Character.HumanoidRootPart.CFrame + Vector3.new(0, -5, 0)
        end)

    elseif b.Text == 'UnHide drop' then
        b.Text = 'Hide drop - 10k'
        getgenv().HideDrop:Disconnect()
        wait()
        workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().OldPos
    end
end)

Main:CreateButton('Hide drop - 100', function(b)
    if b.Text == 'Hide drop - 100' then
        game.ReplicatedStorage.MainEvent:FireServer('Block', true)
        getgenv().OldPos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        b.Text = 'UnHide drop'
        local Now = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        repeat
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Ignored.Shop["[Money Gun] - $777"].Head.CFrame
            fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Money Gun] - $777"].ClickDetector)
            wait()
        until game.Players.LocalPlayer.Backpack:FindFirstChild('[Money Gun]') ~= nil
        game.Players.LocalPlayer.Backpack:FindFirstChild('[Money Gun]').Parent = game.Players.LocalPlayer.Character
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Now
        wait()
        local Tool = game.Players.LocalPlayer.Backpack:FindFirstChild('[Money Gun]') or game.Players.LocalPlayer.Character:FindFirstChild('[Money Gun]')
        
        spawn(function()
            repeat
                Tool.Parent = game.Players.LocalPlayer.Character
                Tool:Activate()
                wait()
            until b.Text == 'Hide drop - 100'
        end)
        
        getgenv().HideDrop = game:GetService("RunService").Heartbeat:Connect(function()
            workspace.CurrentCamera.CameraSubject = getgenv().Player.Character
            game.ReplicatedStorage.MainEvent:FireServer('Block', true)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().Player.Character.HumanoidRootPart.CFrame + Vector3.new(0, -5, 0)
        end)

    elseif b.Text == 'UnHide drop' then
        b.Text = 'Hide drop - 100'
        getgenv().HideDrop:Disconnect()
        wait()
        game.ReplicatedStorage.MainEvent:FireServer('Block', false)
        workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().OldPos
    end
end)


local CashAura = false
Main:CreateButton('CashAura', function(button)
   if button.Text == 'CashAura' then
       button.Text = 'UnCashAura'
       else
           button.Text = 'CashAura'
   end
   CashAura = not CashAura
   _G.CashAura = CashAura
   spawn(function()
       while _G.CashAura == true do
           for i,v in pairs(game:GetService("Workspace").Ignored.Drop:GetChildren()) do 
               if v:IsA("Part") then
                   if (v.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <=30 then
                       fireclickdetector(v.ClickDetector)
                   end
               end
           end
           wait()
       end
   end)
end)

Main:CreateButton('Auto drop', function(button)
    if button.Text == 'Auto drop' then
        button.Text = 'UnAuto drop'
        getgenv().AutoDrop = true
        spawn(function()
            while getgenv().AutoDrop == true do
                game.ReplicatedStorage.MainEvent:FireServer('DropMoney', '10000')
                wait()
            end

        end)
    elseif button.Text == 'UnAuto drop' then
        button.Text = 'Auto drop'
        getgenv().AutoDrop = false
    end
end)

Main:CreateButton('Auto MoneyGun', function(button)
    if button.Text == 'Auto MoneyGun' then
        button.Text = 'UnAuto MoneyGun'
        local MoneyGun = game.Players.LocalPlayer.Backpack:FindFirstChild('[Money Gun]') or game.Players.LocalPlayer.Character:FindFirstChild('[Money Gun]')
        getgenv().AutoMoneyGun = function()
            spawn(function()
                repeat
                    if MoneyGun then
                        MoneyGun.Parent = game.Players.LocalPlayer.Character
                        MoneyGun:Activate()
                    end
                    wait()
                until button.Text == 'Auto MoneyGun'
            end)
        end
        if MoneyGun then
            getgenv().AutoMoneyGun()
        else
            local Now = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            repeat
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Ignored.Shop["[Money Gun] - $777"].Head.CFrame
                fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Money Gun] - $777"].ClickDetector)
                wait()
            until game.Players.LocalPlayer.Backpack:FindFirstChild('[Money Gun]') ~= nil
            game.Players.LocalPlayer.Backpack:FindFirstChild('[Money Gun]').Parent = game.Players.LocalPlayer.Character
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Now
            wait()
            local Now = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            repeat
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Ignored.Shop["[Money Gun] - $777"].Head.CFrame
                fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Money Gun] - $777"].ClickDetector)
                wait()
            until game.Players.LocalPlayer.Backpack:FindFirstChild('[Money Gun]') ~= nil
            game.Players.LocalPlayer.Backpack:FindFirstChild('[Money Gun]').Parent = game.Players.LocalPlayer.Character
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Now
            wait()
            
            wait(.1)
            local MoneyGun = game.Players.LocalPlayer.Backpack:FindFirstChild('[Money Gun]') or game.Players.LocalPlayer.Character:FindFirstChild('[Money Gun]')
            if MoneyGun then
                getgenv().AutoMoneyGun()
            end
        end
    elseif button.Text == 'UnAuto MoneyGun' then
        button.Text = 'Auto MoneyGun'

    end
end)

Main:CreateButton('Freeze', function(button)
    if button.Text == 'Freeze' then
        button.Text = 'UnFreeze'
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
    elseif button.Text == 'UnFreeze' then
        button.Text = 'Freeze'
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
    end
