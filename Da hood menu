    if message == '/e hide' then
        getgenv().MenuOn = false
    end
end)

local PlayersEntered = true
function CheckMoney(quanty, callback)
    local callback = callback or function()end

    if game:GetService("Players").LocalPlayer.DataFolder.Currency.Value >= quanty then
        pcall(callback)
    else
        game.StarterGui:SetCore("SendNotification", {
            Title = 'Error';
            Text = 'You dont have ' .. quanty; 
        })
    end
end

function HideMain()
    if game.ReplicatedStorage:FindFirstChild('MainEvent') then
        game.StarterGui:SetCore("SendNotification", {
            Title = 'Sucess';
            Text = 'Move MainEvent to lighting';
        })
        game.ReplicatedStorage:FindFirstChild('MainEvent').Parent = game.Lighting
       elseif game.Lighting:FindFirstChild('MainEvent') then
        game.StarterGui:SetCore("SendNotification", {
            Title = 'Sucess';
            Text = 'Move MainEvent to ReplicatedStorage';
        })
        game.Lighting.MainEvent.Parent = game.ReplicatedStorage   
    end
end

