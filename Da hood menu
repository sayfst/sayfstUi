                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(character.HumanoidRootPart.Position)
                    elseif args[2] == game.Players.LocalPlayer.Name or args[2] ==  game.Players.LocalPlayer.DisplayName then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(character.HumanoidRootPart.Position)
                    end
                end
                if args[1] == prefix .. 'freeze' then
