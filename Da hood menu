        Wanted.Position = UDim2.new(0.599309623, 0, 0.826271117, 0)
        Wanted.Selectable = true
        Wanted.Size = UDim2.new(0, 177, 0, 21)
        Wanted.Font = Enum.Font.SourceSans
        Wanted.Text = "Wanted:"
        Wanted.TextColor3 = Color3.fromRGB(255, 255, 255)
        Wanted.TextSize = 14.000
    
        -- Scripts:
    
        local function KTPQIKD_fake_script() -- TextBox.LocalScript 
            local script = Instance.new('LocalScript', TextBox)
    
            script.Parent.FocusLost:Connect(function(a)
                if a then
                    local player = script.Parent.Text
                    for i,v in pairs(game.Players:GetChildren()) do
                        if string.find(v.Name, player) or string.find(v.DisplayName, player) then
                            script.Parent.Text = v.Name
                            local pr = v
            
                            local UserId = pr.UserId
                            local ThumbType = Enum.ThumbnailType.HeadShot
                            local ThumbSize = Enum.ThumbnailSize.Size150x150
            
                            script.Parent.Parent.ImageLabel.Image = game.Players:GetUserThumbnailAsync(UserId,ThumbType,ThumbSize)
                            script.Parent.Parent.Namee.Text = 'Name: '..v.Name
                            script.Parent.Parent.Money.Text = 'Money: '..v.DataFolder.Currency.Value
                            script.Parent.Parent.Wanted.Text = 'Wanted: '..v.leaderstats.Wanted.Value
                        end
                    end
                end
            end)
        end
        coroutine.wrap(KTPQIKD_fake_script)()
        local function LDOBAYA_fake_script() -- Select.LocalScript 
            local script = Instance.new('LocalScript', Select)
    
            script.Parent.Parent.Parent.Frame.Active = true
            script.Parent.Parent.Parent.Frame.Draggable = true
            
            script.Parent.MouseButton1Click:Connect(function()
                getgenv().Player = game.Players:FindFirstChild(script.Parent.Parent.TextBox.Text)
            end)
        end
        coroutine.wrap(LDOBAYA_fake_script)()
    
    end)
end)

Main:CreateButton('Player aura', function()
-- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local Name = Instance.new("TextButton")
local USer = Instance.new("TextButton")

--Properties:

ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 106, 106)
Frame.Position = UDim2.new(0.489287496, 0, 0.438741744, 0)
Frame.Size = UDim2.new(0, 474, 0, 182)
Frame.Active = true
Frame.Draggable = true

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 179, 179)
TextLabel.Position = UDim2.new(0.0189873427, 0, 0.049450554, 0)
TextLabel.Size = UDim2.new(0, 455, 0, 118)
TextLabel.Font = Enum.Font.SourceSans
TextLabel.Text = "nil"
TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.TextScaled = true
TextLabel.TextSize = 14.000
TextLabel.TextWrapped = true

Name.Name = "Name"
Name.Parent = Frame
Name.BackgroundColor3 = Color3.fromRGB(255, 161, 161)
Name.Position = UDim2.new(0.0738396645, 0, 0.769230783, 0)
Name.Size = UDim2.new(0, 190, 0, 27)
Name.Font = Enum.Font.SourceSans
Name.Text = "Copyname"
Name.TextColor3 = Color3.fromRGB(0, 0, 0)
Name.TextScaled = true
Name.TextSize = 14.000
Name.TextWrapped = true

USer.Name = "USer"
USer.Parent = Frame
USer.BackgroundColor3 = Color3.fromRGB(255, 161, 161)
USer.Position = UDim2.new(0.508438826, 0, 0.769230783, 0)
USer.Size = UDim2.new(0, 190, 0, 27)
USer.Font = Enum.Font.SourceSans
USer.Text = "Update"
USer.TextColor3 = Color3.fromRGB(0, 0, 0)
USer.TextScaled = true
USer.TextSize = 14.000
USer.TextWrapped = true

-- Scripts:

local function HUNZOC_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)
    script.Parent.Parent.USer.MouseButton1Click:Connect(function()
        spawn(function()
            while true do
                for i,v in pairs(game.Players:GetChildren()) do
                    
                    if v ~= game.Players.LocalPlayer then
                        character = v.Character
                        if (character.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 10 then
                            script.Parent.Parent.TextLabel.Text = v.Name..'/'..game.Players:FindFirstChild(v.Name).UserId
                        end
                    end
                    
                end
                wait(.3)
            end
        end)
    end)
	while true do
		for i,v in pairs(game.Players:GetChildren()) do
			
			if v ~= game.Players.LocalPlayer then
				character = v.Character
				if (character.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 10 then
					script.Parent.Parent.TextLabel.Text = v.Name..'/'..game.Players:FindFirstChild(v.Name).UserId
				end
			end
			
		end
		wait(.3)
	end
end
coroutine.wrap(HUNZOC_fake_script)()
local function DJJSPDP_fake_script() -- Name.LocalScript 
	local script = Instance.new('LocalScript', Name)

	script.Parent.MouseButton1Click:Connect(function()
		local args = script.Parent.Parent.TextLabel.Text:split('/')
		setclipboard(args[1])
	end)
end
coroutine.wrap(DJJSPDP_fake_script)()
local function ZJLP_fake_script() -- USer.LocalScript 
	local script = Instance.new('LocalScript', USer)

	script.Parent.MouseButton1Click:Connect(function()
		local args = script.Parent.Parent.TextLabel.Text:split('/')
		setclipboard(args[2])
	end)
end
coroutine.wrap(ZJLP_fake_script)()

end)

Main:CreateButton('Tool select', function()
   local Tool = Instance.new("Tool", game.Players.LocalPlayer.Backpack)
   Tool.RequiresHandle = false
   Tool.Name = 'select player'
   local mouse = game.Players.LocalPlayer:GetMouse()
       
   Tool.Activated:Connect(function()
       local target = mouse.Target
       if game.Players:FindFirstChild(target.Parent.Name) then
           local target = mouse.Target
           getgenv().Player = game.Players:FindFirstChild(target.Parent.Name)
           game.StarterGui:SetCore("SendNotification", {
               Title = 'Sucess';
               Text = target.Parent.Name
           })    
       end
   end)
end)

Main:CreateButton('Select players', function()
-- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextButton = Instance.new("TextButton")
local UIGridLayout = Instance.new("UIGridLayout")

--Properties:

ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

spawn(function()
    while true do
        if getgenv().MenuOn == false then
            ScreenGui:Destroy()
        end
        wait()
    end
end)
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
Frame.Position = UDim2.new(0.26628077, 0, 0.610129595, 0)
Frame.Size = UDim2.new(0, 645, 0, 311)
Frame.Active = true
Frame.Draggable = true

TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
TextButton.BorderSizePixel = 0
TextButton.Size = UDim2.new(0, 200, 0, 50)
TextButton.Visible = false
TextButton.Font = Enum.Font.SourceSans
TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
TextButton.TextSize = 14.000

UIGridLayout.Parent = Frame
UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
UIGridLayout.CellSize = UDim2.new(0, 76, 0, 30)

-- Scripts:

local function ZWPY_fake_script() -- Frame.LocalScript 
    local script = Instance.new('LocalScript', Frame)

    for i,v in pairs(game.Players:GetChildren()) do
        if v ~= game.Players.LocalPlayer then
            local clone = script.Parent.TextButton:Clone()
            clone.Parent = script.Parent
            clone.Name = v.Name
            clone.TextSize = 14
            clone.Text = v.Name 
            clone.Visible = true
            clone.MouseButton1Click:Connect(function()
                getgenv().Player = game.Players:FindFirstChild(v.Name)
            end)
        end
    end
    
    game.Players.ChildAdded:Connect(function(v)
        local clone = script.Parent.TextButton:Clone()
        clone.Parent = script.Parent
        clone.Name = v.Name
        clone.TextSize = 12
        clone.Text = v.Name
        clone.Visible = true
        clone.MouseButton1Click:Connect(function()
            getgenv().Player = game.Players:FindFirstChild(v.Name)
        end)
    end)
    game.Players.ChildRemoved:Connect(function(v)
        script.Parent:FindFirstChild(v.Name):Destroy()
    end)
end
coroutine.wrap(ZWPY_fake_script)()

end)

Main:CreateButton('eduaeto', function(button)
   getgenv().Player = game.Players:FindFirstChild(button.Text) 
end)
Main:CreateButton('MinhasContas71', function(button)
   getgenv().Player = game.Players:FindFirstChild(button.Text) 
end)

game.Players.LocalPlayer.Chatted:Connect(function(message)
   args = message:split(' ')

   if args[1] == ':add' then
       if game.Players:FindFirstChild(args[2]) then
           Main:CreateButton(args[2], function(button)
               getgenv().Player = game.Players:FindFirstChild(button.Text) 
               game.StarterGui:SetCore("SendNotification", {
                   Title = 'Sucess added';
                   Text = button.Text;
               })
           end)
           game.Players.LocalPlayer.Chatted:Connect(function(message)
               args2 = message:split(' ')
               if args2[1] == ':remove' then
                   if args2[2] == button.Text then
                       button:Destroy()
                   end
               end
           end)
       end
   end
end)
