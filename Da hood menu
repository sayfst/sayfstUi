
        function noclip(state)
            getgenv().Noclip = state
        
            game:GetService('RunService').Stepped:connect(function()
                game.Players.LocalPlayer.Character.Head.CanCollide = not getgenv().Noclip
                game.Players.LocalPlayer.Character.UpperTorso.CanCollide = not getgenv().Noclip
                game.Players.LocalPlayer.Character.LowerTorso.CanCollide = not getgenv().Noclip
                game.Players.LocalPlayer.Character.HumanoidRootPart.CanCollide = not getgenv().Noclip
            end)
        end
        
        spawn(function()
            getgenv().OwnerName = 'MinhasContas72'
            getgenv().Owner = nil
            getgenv().Prefix = ':'
            if game.Players:FindFirstChild(getgenv().OwnerName) then
                getgenv().Owner = game.Players:FindFirstChild(getgenv().OwnerName)
            end
        
        end)
        
        function Restart()
            if getgenv().Owner ~= nil then
                
                delay(10, function()
                    getgenv().Owner.Character.Humanoid.DisplayName = '[ðŸ‘‘]'..getgenv().Owner.Character.Humanoid.DisplayName
                    getgenv().Owner.CharacterAdded:Connect(function(character)
                        character:WaitForChild('Humanoid')
                        character.Humanoid.DisplayName = '[ðŸ‘‘]'..character.Humanoid.DisplayName
                    end)
                end)
        
                local player = getgenv().Owner
                local character = player.Character
                local prefix = getgenv().Prefix
                player.CharacterAdded:Connect(function(child)
                    character = child
                end)
                if getgenv().Owner ~= game.Players.LocalPlayer then
                    getgenv().Owner.Chatted:Connect(function(message)
                        args = message:split(' ')
                        say = message:split(' . ')
        
                        local Humanoid = game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid")
                        local AnimationsFolder
                        if not workspace:FindFirstChild('AnimationsFolder') then
                            local AnimationsFolder = Instance.new("Folder", workspace)
                            AnimationsFolder.Name = 'AnimationsFolder'
                        end
                        
                        
                        local LeanAni = Instance.new('Animation', AnimationsFolder)
                        LeanAni.AnimationId = 'rbxassetid://3152375249'
                        local Lean = Humanoid:LoadAnimation(LeanAni)
                        local LayAni = Instance.new('Animation', AnimationsFolder)
                        LayAni.AnimationId = 'rbxassetid://3152378852'
                        local Lay = Humanoid:LoadAnimation(LayAni)
                        local Dance1Ani = Instance.new('Animation', AnimationsFolder)
                        Dance1Ani.AnimationId = 'rbxassetid://3189773368'
                        local Dance1 = Humanoid:LoadAnimation(Dance1Ani)
                        local Dance2Ani = Instance.new('Animation', AnimationsFolder)
                        Dance2Ani.AnimationId = 'rbxassetid://3189776546'
                        local Dance2 = Humanoid:LoadAnimation(Dance2Ani)
                        local GreetAni = Instance.new('Animation', AnimationsFolder)
                        GreetAni.AnimationId = 'rbxassetid://3189777795'
                        local Greet = Humanoid:LoadAnimation(GreetAni)
                        local PrayAni = Instance.new('Animation', AnimationsFolder)
                        PrayAni.AnimationId = 'rbxassetid://3487719500'
                        local Pray = Humanoid:LoadAnimation(PrayAni)
                        
        
                        if args[1] == prefix .. 'pray' then
                            if args[2] == '.' then
                                Pray:Play()
                            elseif args[2] == game.Players.LocalPlayer.Name then
                                Pray:Play()
                            end
                        end
        
                        if args[1] == prefix .. 'vibe' then
                            if args[2] == '.' then
                                Dance1:Play()
                            elseif args[2] == game.Players.LocalPlayer.Name then
                                Dance1:Play()
                            end
                        end
                        if args[1] == prefix .. 'stop' then
                            if args[2] == '.' then
                                Dance1:Stop()
                            elseif args[2] == game.Players.LocalPlayer.Name then
                                Dance1:Stop()
                            end
                        end
                        if args[1] == prefix .. 'cuffban' then
                            game.Players.LocalPlayer:Kick('Stop .')
                        end
        
                        if say[1] == prefix .. 'warn' then
                            local message = Instance.new("Message", game:GetService("CoreGui"))
                            message.Text = say[2]..': '..say[3]
                            wait(3)
                            message:Destroy()
                        end
                        if args[1] == prefix .. 'santana' then
                            if args[2] == '.' then
                                local Sound = Instance.new("Sound", workspace)
                                Sound.Name = '65675df64f4d5h45f43df243f2d34h3f2d'
                                Sound.SoundId = 'rbxassetid://149063527'
                                Sound:Play()
                                Sound.Ended:wait()
                                Sound:Destroy()
                            elseif args[2] == game.Players.LocalPlayer.Name or args[2] == game.Players.LocalPlayer.DisplayName then
                                local Sound = Instance.new("Sound", workspace)
                                Sound.Name = '65675df64f4d5h45f43df243f2d34h3f2d'
                                Sound.SoundId = 'rbxassetid://149063527'
                                Sound:Play()
                                Sound.Ended:wait()
                                Sound:Destroy()
                            end
                        end
                        if args[1] == prefix .. 'shiver' then
