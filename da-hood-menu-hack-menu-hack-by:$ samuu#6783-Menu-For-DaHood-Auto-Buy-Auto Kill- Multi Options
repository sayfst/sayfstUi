local library = {}

if not game:IsLoaded() then
    game.Loaded:wait()
    setclipboard('Owner of menu: $ samuu#6783, Who inspired me: wyv#6626')
    game.StarterGui:SetCore("SendNotification", {
        Title = 'Sucess loaded Da hood menu';
        Text = 'Credits have been copied to your clipboard';
        Duration = 30;
    })
    wait(1)
    game.StarterGui:SetCore("SendNotification", {
        Title = 'Da hood';
        Text = 'Press "f1" to toggle menu visible';
        Duration = 10
    })
    
end


